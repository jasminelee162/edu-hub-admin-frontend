\n      <div class=\"search-table\">\r\n        <div class=\"search\">\r\n            <el-row :gutter=\"10\" style=\"padding:10px\">\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\"><i class=\"el-icon-user\"></i> 用户昵称:</span>\r\n                    <el-input\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\"\r\n                        placeholder=\"请输入用户昵称\"\r\n                        v-model=\"search.userName\">\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\"><i class=\"el-icon-chat-dot-round\"></i> 提问内容:</span>\r\n                    <el-input\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\"\r\n                        placeholder=\"请输入提问内容\"\r\n                        v-model=\"search.content\">\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\"><i class=\"el-icon-chat-line-round\"></i> 回复:</span>\r\n                    <el-input\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\"\r\n                        placeholder=\"请输入回复\"\r\n                        v-model=\"search.answer\">\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\"><i class=\"el-icon-notebook-2\"></i> 课程名称:</span>\r\n                    <el-input\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\"\r\n                        placeholder=\"请输入课程名称\"\r\n                        v-model=\"search.taskName\">\r\n                    </el-input>\r\n                </el-col>\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <el-button style=\"margin-top:10px\" size=\"mini\" icon=\"el-icon-search\" type=\"primary\" @click=\"searchPage\">查询</el-button>\r\n                    <el-button style=\"margin-top:10px\" size=\"mini\" icon=\"el-icon-refresh\" @click=\"refresh\">重置</el-button>\r\n                </el-col>\r\n            </el-row>\r\n        </div>\r\n        <div class=\"table\">\r\n            <el-row style=\"padding-top:10px;margin-left:10px\">\r\n                <el-button type=\"danger\" :disabled=\"update.length <= 0 ?true:false\" size=\"mini\" icon=\"el-icon-delete\" plain @click=\"deleteDataBtn\">删除</el-button>\r\n            </el-row>\r\n            <el-table\r\n            v-loading=\"loading\"\r\n            :data=\"tableData\"\r\n            :header-cell-style=\"{\r\n              'color': '#4A2B90',\r\n              'background-color': '#F0EBFF',\r\n              'font-weight': '600',\r\n              'border-bottom': '1px solid #4A2B90'\r\n            }\"\r\n            :row-style=\"{\r\n              'color': '#333',\r\n              'font-size': '14px',\r\n              'font-family':'Microsoft YaHei',\r\n              'white-space': 'nowrap',\r\n              'border-bottom': '1px solid #EBEEF5'\r\n            }\"\r\n            @selection-change=\"handleSelectionChange\"\r\n            stripe\r\n            style=\"width: 100%\">\r\n            <el-table-column\r\n              type=\"selection\"\r\n              width=\"55\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"userName\"\r\n              label=\"用户昵称\"\r\n              >\r\n              <template slot-scope=\"scope\">\r\n                <span><i class=\"el-icon-user-solid\" style=\"color:#4A2B90;margin-right:5px\"></i>{{scope.row.userName}}</span>\r\n              </template>\r\n            </el-table-column>\