{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getApeTestPage, removeApeTest, editApeTest } from '../../../api/api';\nimport add from '../../../components/system/testP/addTest';\nimport update from '../../../components/system/testP/updateTest';\nexport default {\n  data() {\n    return {\n      loading: true,\n      update: [],\n      remove: [],\n      updateId: \"\",\n      addVisible: false,\n      updateVisible: false,\n      search: {\n        name: \"\",\n        taskName: \"\",\n        state: \"\",\n        createBy: \"\",\n        pageNumber: 1,\n        pageSize: 10,\n        type: 1\n      },\n      total: 0,\n      tableData: []\n    };\n  },\n  components: {\n    add,\n    update\n  },\n  methods: {\n    tableHeaderStyle() {\n      return {\n        'color': '#4A2B90',\n        'background-color': '#F0EBFF',\n        'font-weight': '600',\n        'border-bottom': '1px solid #4A2B90'\n      };\n    },\n    tableRowStyle() {\n      return {\n        'color': '#333',\n        'font-size': '14px',\n        'border-bottom': '1px solid #EBEEF5'\n      };\n    },\n    toTestItem(id) {\n      var param = {\n        \"name\": \"考试题目\",\n        \"url\": \"/testItem?id=\" + id\n      };\n      this.$store.commit('menu/addActiveMenu', param);\n      this.$router.push(\"/testItem?id=\" + id);\n      this.$store.commit('menu/setActiveMenu', \"/testItem?id=\" + id);\n    },\n    toTestStudent(id) {\n      var param = {\n        \"name\": \"阅卷\",\n        \"url\": \"/testStudent?id=\" + id\n      };\n      this.$store.commit('menu/addActiveMenu', param);\n      this.$router.push(\"/testStudent?id=\" + id);\n      this.$store.commit('menu/setActiveMenu', \"/testStudent?id=\" + id);\n    },\n    listing(id) {\n      var param = {\n        id: id,\n        state: 0\n      };\n      editApeTest(param).then(res => {\n        if (res.code == 1000) {\n          this.$notify.success({\n            title: '成功',\n            message: \"审核成功\"\n          });\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    editApeTest(id) {\n      var param = {\n        id: id,\n        state: 1\n      };\n      editApeTest(param).then(res => {\n        if (res.code == 1000) {\n          this.$notify.success({\n            title: '成功',\n            message: \"审核成功\"\n          });\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    searchPage() {\n      this.search.pageNumber = 1;\n      this.query();\n    },\n    query() {\n      getApeTestPage(this.search).then(res => {\n        if (res.code == 1000) {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n          this.loading = false;\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    refresh() {\n      this.search.name = \"\";\n      this.search.taskName = \"\";\n      this.search.state = \"\";\n      this.search.createBy = \"\";\n      this.query();\n    },\n    handleCurrentChange(val) {\n      this.search.pageNumber = val;\n      this.query();\n    },\n    handleSizeChange(val) {\n      this.search.pageSize = val;\n      this.query();\n    },\n    handleSelectionChange(val) {\n      this.update = [];\n      this.remove = [];\n      for (let i = 0; i < val.length; i++) {\n        var item = val[i];\n        this.update.push(item.id);\n        this.remove.push(item.id);\n      }\n    },\n    add() {\n      this.addVisible = true;\n    },\n    addFalse() {\n      this.addVisible = false;\n      this.query();\n    },\n    updateFalse() {\n      this.updateId = '';\n      this.updateVisible = false;\n      this.query();\n    },\n    updateData(id) {\n      this.updateId = id;\n      this.updateVisible = true;\n    },\n    updateDataBtn() {\n      this.updateData(this.update[0]);\n    },\n    deleteDataBtn() {\n      this.$confirm('确定删除选中的' + this.remove.length + '条数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        customClass: 'tech-message-box'\n      }).then(() => {\n        this.deleteDate(this.remove.join(\",\"));\n      }).catch(() => {});\n    },\n    deleteDate(ids) {\n      removeApeTest({\n        ids: ids\n      }).then(res => {\n        if (res.code == 1000) {\n          this.$message({\n            type: 'success',\n            message: '删除成功!',\n            customClass: 'tech-message'\n          });\n          this.pageNumber = 1;\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["getApeTestPage","removeApeTest","editApeTest","add","update","data","loading","remove","updateId","addVisible","updateVisible","search","name","taskName","state","createBy","pageNumber","pageSize","type","total","tableData","components","methods","tableHeaderStyle","tableRowStyle","toTestItem","id","param","$store","commit","$router","push","toTestStudent","listing","then","res","code","$notify","success","title","message","query","error","searchPage","records","refresh","handleCurrentChange","val","handleSizeChange","handleSelectionChange","i","length","item","addFalse","updateFalse","updateData","updateDataBtn","deleteDataBtn","$confirm","confirmButtonText","cancelButtonText","customClass","deleteDate","join","catch","ids","$message","mounted"],"sources":["src/views/system/testP/teacherTest.vue"],"sourcesContent":["<template>\r\n  <div class=\"test-management\">\r\n    <div class=\"search-panel\">\r\n      <el-row :gutter=\"15\">\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-document\"></i> 考试名称:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入考试名称\"\r\n              v-model=\"search.name\"\r\n              class=\"tech-input\">\r\n              <i slot=\"prefix\" class=\"el-icon-edit-outline\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-notebook-2\"></i> 课程名称:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入课程名称\"\r\n              v-model=\"search.taskName\"\r\n              class=\"tech-input\">\r\n              <i slot=\"prefix\" class=\"el-icon-collection\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-collection-tag\"></i> 审核状态:\r\n            </span>\r\n            <el-select \r\n              clearable \r\n              size=\"small\" \r\n              v-model=\"search.state\" \r\n              placeholder=\"请选择\"\r\n              class=\"tech-select\">\r\n              <el-option\r\n                label=\"审核通过\"\r\n                value=\"0\">\r\n                <i class=\"el-icon-success\" style=\"color:#67C23A;margin-right:5px\"></i>审核通过\r\n              </el-option>\r\n              <el-option\r\n                label=\"未通过\"\r\n                value=\"1\">\r\n                <i class=\"el-icon-error\" style=\"color:#F56C6C;margin-right:5px\"></i>未通过\r\n              </el-option>\r\n              <el-option\r\n                label=\"待审核\"\r\n                value=\"2\">\r\n                <i class=\"el-icon-warning-outline\" style=\"color:#E6A23C;margin-right:5px\"></i>待审核\r\n              </el-option>\r\n            </el-select>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-user\"></i> 创建者:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入创建者\"\r\n              v-model=\"search.createBy\"\r\n              class=\"tech-input\">\r\n              <i slot=\"prefix\" class=\"el-icon-user-solid\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-actions\">\r\n            <el-button \r\n              size=\"small\" \r\n              icon=\"el-icon-search\" \r\n              type=\"primary\" \r\n              @click=\"searchPage\"\r\n              class=\"search-btn\">\r\n              查询\r\n            </el-button>\r\n            <el-button \r\n              size=\"small\" \r\n              icon=\"el-icon-refresh\" \r\n              @click=\"refresh\"\r\n              class=\"reset-btn\">\r\n              重置\r\n            </el-button>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <div class=\"data-panel\">\r\n      <div class=\"action-bar\">\r\n        <el-button \r\n          type=\"primary\" \r\n          size=\"small\" \r\n          icon=\"el-icon-plus\" \r\n          @click=\"add\"\r\n          class=\"action-btn add-btn\">\r\n          新增\r\n        </el-button>\r\n        <el-button \r\n          type=\"success\" \r\n          size=\"small\" \r\n          icon=\"el-icon-edit\" \r\n          :disabled=\"update.length !== 1\"\r\n          @click=\"updateDataBtn\"\r\n          class=\"action-btn edit-btn\">\r\n          修改\r\n        </el-button>\r\n        <el-button \r\n          type=\"danger\" \r\n          size=\"small\" \r\n          icon=\"el-icon-delete\" \r\n          :disabled=\"update.length <= 0\"\r\n          @click=\"deleteDataBtn\"\r\n          class=\"action-btn delete-btn\">\r\n          删除\r\n        </el-button>\r\n      </div>\r\n\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"tableData\"\r\n        :header-cell-style=\"tableHeaderStyle\"\r\n        :row-style=\"tableRowStyle\"\r\n        @selection-change=\"handleSelectionChange\"\r\n        stripe\r\n        class=\"tech-table\">\r\n        <el-table-column \r\n          type=\"selection\" \r\n          width=\"55\"\r\n          align=\"center\">\r\n        </el-table-column>\r\n        <el-table-column \r\n          prop=\"name\" \r\n          label=\"考试名称\"\r\n          width=\"180\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"name-cell\">\r\n              <i class=\"el-icon-document\"></i>\r\n              <span>{{ row.name }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column \r\n          prop=\"taskName\" \r\n          label=\"课程名称\"\r\n          width=\"180\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"task-cell\">\r\n              <i class=\"el-icon-collection\"></i>\r\n              <span>{{ row.taskName }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column \r\n          prop=\"totalScore\" \r\n          label=\"总分\"\r\n          width=\"100\"\r\n          align=\"center\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"score-cell\">\r\n              <i class=\"el-icon-medal\"></i>\r\n              <span>{{ row.totalScore }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column \r\n          prop=\"totalTime\" \r\n          label=\"考试时长\"\r\n          width=\"100\"\r\n          align=\"center\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"time-cell\">\r\n              <i class=\"el-icon-timer\"></i>\r\n              <span>{{ row.totalTime }}分钟</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column \r\n          prop=\"state\" \r\n          label=\"状态\"\r\n          width=\"120\"\r\n          align=\"center\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"state-cell\">\r\n              <el-tag v-if=\"row.state == 0\" type=\"success\" effect=\"dark\">\r\n                <i class=\"el-icon-success\"></i> 通过\r\n              </el-tag>\r\n              <el-tag v-if=\"row.state == 1\" type=\"danger\" effect=\"dark\">\r\n                <i class=\"el-icon-error\"></i> 未通过\r\n              </el-tag>\r\n              <el-tag v-if=\"row.state == 2\" type=\"warning\" effect=\"dark\">\r\n                <i class=\"el-icon-warning-outline\"></i> 待审核\r\n              </el-tag>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column \r\n          prop=\"createBy\" \r\n          label=\"创建者\"\r\n          width=\"120\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"user-cell\">\r\n              <i class=\"el-icon-user\"></i>\r\n              <span>{{ row.createBy }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column \r\n          label=\"操作\" \r\n          width=\"400\"\r\n          fixed=\"right\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"action-buttons\">\r\n              <el-button \r\n                size=\"mini\" \r\n                @click=\"toTestStudent(row.id)\"\r\n                class=\"action-btn grade-btn\">\r\n                <i class=\"el-icon-reading\"></i> 阅卷\r\n              </el-button>\r\n              <el-button \r\n                size=\"mini\" \r\n                @click=\"toTestItem(row.id)\"\r\n                class=\"action-btn question-btn\">\r\n                <i class=\"el-icon-tickets\"></i> 考试题目\r\n              </el-button>\r\n              <el-button \r\n                size=\"mini\" \r\n                @click=\"updateData(row.id)\"\r\n                class=\"action-btn edit-btn\">\r\n                <i class=\"el-icon-edit\"></i> 修改\r\n              </el-button>\r\n              <el-popconfirm\r\n                title=\"确认删除这条考试记录？\"\r\n                @confirm=\"deleteDate(row.id)\"\r\n                confirm-button-text=\"确认\"\r\n                cancel-button-text=\"取消\"\r\n                icon=\"el-icon-warning\"\r\n                icon-color=\"#7B68EE\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  slot=\"reference\" \r\n                  class=\"action-btn delete-btn\">\r\n                  <i class=\"el-icon-delete\"></i> 删除\r\n                </el-button>\r\n              </el-popconfirm>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <el-pagination\r\n        background\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        :page-size=\"search.pageSize\"\r\n        :current-page=\"search.pageNumber\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :total=\"total\"\r\n        class=\"tech-pagination\">\r\n      </el-pagination>\r\n    </div>\r\n\r\n    <add @addFalse=\"addFalse\" flag=\"2\" :addVisible=\"addVisible\"></add>\r\n    <update @updateFalse=\"updateFalse\" flag=\"2\" :updateId=\"updateId\" :updateVisible=\"updateVisible\"></update>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import {getApeTestPage,removeApeTest,editApeTest} from '../../../api/api' \r\n  import add from '../../../components/system/testP/addTest'\r\n  import update from '../../../components/system/testP/updateTest'\r\n  export default {\r\n    data() {\r\n      return{\r\n        loading: true,\r\n        update: [],\r\n        remove: [],\r\n        updateId: \"\",\r\n        addVisible: false,\r\n        updateVisible: false,\r\n        search: {\r\n            name: \"\",\r\n            taskName: \"\",\r\n            state: \"\",\r\n            createBy: \"\",\r\n            pageNumber: 1,\r\n            pageSize:10,\r\n            type: 1\r\n        },\r\n        total: 0,\r\n        tableData: []\r\n      }\r\n    },\r\n    components: {\r\n      add,\r\n      update\r\n    },\r\n    methods: {\r\n      tableHeaderStyle() {\r\n        return {\r\n          'color': '#4A2B90',\r\n          'background-color': '#F0EBFF',\r\n          'font-weight': '600',\r\n          'border-bottom': '1px solid #4A2B90'\r\n        }\r\n      },\r\n      tableRowStyle() {\r\n        return {\r\n          'color': '#333',\r\n          'font-size': '14px',\r\n          'border-bottom': '1px solid #EBEEF5'\r\n        }\r\n      },\r\n      toTestItem(id) {\r\n        var param = {\r\n            \"name\": \"考试题目\",\r\n            \"url\": \"/testItem?id=\" + id\r\n        }\r\n        this.$store.commit('menu/addActiveMenu', param)\r\n        this.$router.push(\"/testItem?id=\" + id)\r\n        this.$store.commit('menu/setActiveMenu', \"/testItem?id=\" + id)\r\n      },\r\n      toTestStudent(id) {\r\n        var param = {\r\n            \"name\": \"阅卷\",\r\n            \"url\": \"/testStudent?id=\" + id\r\n        }\r\n        this.$store.commit('menu/addActiveMenu', param)\r\n        this.$router.push(\"/testStudent?id=\" + id)\r\n        this.$store.commit('menu/setActiveMenu', \"/testStudent?id=\" + id)\r\n      },\r\n      listing(id) {\r\n        var param = {\r\n          id: id,\r\n          state: 0\r\n        }\r\n        editApeTest(param).then(res => {\r\n 