{"ast":null,"code":"import { getAdoptTaskStudent } from '../../../api/api';\nexport default {\n  data() {\n    return {\n      loading: false,\n      search: {\n        taskId: '',\n        userName: '',\n        pageNumber: 1,\n        pageSize: 10\n      },\n      total: 0,\n      tableData: []\n    };\n  },\n  methods: {\n    getScoreType(percent) {\n      if (percent >= 0.85) return 'success';\n      if (percent >= 0.6) return 'warning';\n      return 'danger';\n    },\n    getScore(row) {\n      const taskScore = (row.videoCount + row.assignCount) / (row.assign + row.videoNum) * row.proportion;\n      const testScore = row.testScore / row.totalScore * (100 - row.proportion) || 0;\n      return (taskScore + testScore).toFixed(2);\n    },\n    tableHeaderStyle() {\n      return {\n        color: '#4A2B90',\n        'background-color': '#ECE9F4',\n        'font-weight': 'bold',\n        'border-bottom': '1px solid #7B68EE'\n      };\n    },\n    tableRowStyle() {\n      return {\n        color: '#5F4B8B',\n        'font-size': '14px',\n        'border-bottom': '1px solid #F0EEF7'\n      };\n    },\n    searchPage() {\n      this.search.pageNumber = 1;\n      this.query();\n    },\n    query() {\n      this.loading = true;\n      // 这里taskId从路由取，确保参数完整\n      this.search.taskId = this.$route.query.id || this.search.taskId;\n      getAdoptTaskStudent(this.search).then(res => {\n        if (res.code === 1000) {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n        this.loading = false;\n      }).catch(() => {\n        this.loading = false;\n      });\n    },\n    refresh() {\n      this.search.userName = '';\n      this.search.pageNumber = 1;\n      this.query();\n    },\n    handleCurrentChange(val) {\n      this.search.pageNumber = val;\n      this.query();\n    },\n    handleSizeChange(val) {\n      this.search.pageSize = val;\n      this.query();\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["getAdoptTaskStudent","data","loading","search","taskId","userName","pageNumber","pageSize","total","tableData","methods","getScoreType","percent","getScore","row","taskScore","videoCount","assignCount","assign","videoNum","proportion","testScore","totalScore","toFixed","tableHeaderStyle","color","tableRowStyle","searchPage","query","$route","id","then","res","code","records","$notify","error","title","message","catch","refresh","handleCurrentChange","val","handleSizeChange","mounted"],"sources":["src/views/system/task/studyInfo.vue"],"sourcesContent":["<template>\r\n  <div class=\"score-management\">\r\n    <div class=\"search-panel\">\r\n      <el-row :gutter=\"15\">\r\n        <!-- 学生姓名搜索 -->\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-user\"></i> 学生姓名:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入学生姓名\"\r\n              v-model=\"search.userName\"\r\n              @keyup.enter.native=\"searchPage\"\r\n              class=\"tech-input\"\r\n            >\r\n              <i slot=\"prefix\" class=\"el-icon-user-solid\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n\r\n        <!-- 查询和重置按钮 -->\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-actions\">\r\n            <el-button\r\n              size=\"small\"\r\n              icon=\"el-icon-search\"\r\n              type=\"primary\"\r\n              @click=\"searchPage\"\r\n              class=\"search-btn\"\r\n            >\r\n              查询\r\n            </el-button>\r\n            <el-button\r\n              size=\"small\"\r\n              icon=\"el-icon-refresh\"\r\n              @click=\"refresh\"\r\n              class=\"reset-btn\"\r\n            >\r\n              重置\r\n            </el-button>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <div class=\"data-panel\">\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"tableData\"\r\n        :header-cell-style=\"tableHeaderStyle\"\r\n        :row-style=\"tableRowStyle\"\r\n        stripe\r\n        class=\"tech-table\"\r\n      >\r\n        <el-table-column\r\n          type=\"selection\"\r\n          width=\"55\"\r\n          align=\"center\"\r\n        >\r\n        </el-table-column>\r\n        <el-table-column prop=\"userName\" label=\"学生姓名\" width=\"120\">\r\n          <template #default=\"{ row }\">\r\n            <div class=\"user-cell\">\r\n              <i\r\n                class=\"el-icon-user\"\r\n                style=\"color:#9370DB; margin-right:8px\"\r\n              ></i>\r\n              <span>{{ row.userName }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"taskName\" label=\"课程名称\" min-width=\"180\">\r\n          <template #default=\"{ row }\">\r\n            <div class=\"course-cell\">\r\n              <i\r\n                class=\"el-icon-notebook-2\"\r\n                style=\"color:#A5A4BF; margin-right:8px\"\r\n              ></i>\r\n              <span>{{ row.taskName }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"totalScore\"\r\n          label=\"考试总分\"\r\n          width=\"100\"\r\n          align=\"center\"\r\n        >\r\n          <template #default=\"{ row }\">\r\n            <el-tag type=\"info\" effect=\"plain\">{{ row.totalScore }}</el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"testScore\"\r\n          label=\"考试得分\"\r\n          width=\"100\"\r\n          align=\"center\"\r\n        >\r\n          <template #default=\"{ row }\">\r\n            <el-tag\r\n              :type=\"getScoreType(row.testScore / row.totalScore)\"\r\n              effect=\"plain\"\r\n            >\r\n              {{ row.testScore }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"videoCount\"\r\n          label=\"视频得分\"\r\n          width=\"100\"\r\n          align=\"center\"\r\n        >\r\n          <template #default=\"{ row }\">\r\n            <el-tag type=\"primary\" effect=\"plain\">\r\n              <i class=\"el-icon-video-camera\" style=\"margin-right:4px\"></i>\r\n              {{ row.videoCount }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"assignCount\"\r\n          label=\"作业得分\"\r\n          width=\"100\"\r\n          align=\"center\"\r\n        >\r\n          <template #default=\"{ row }\">\r\n            <el-tag type=\"primary\" effect=\"plain\">\r\n              <i class=\"el-icon-document\" style=\"margin-right:4px\"></i>\r\n              {{ row.assignCount }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"综合成绩\" width=\"120\" align=\"center\">\r\n          <template #default=\"{ row }\">\r\n            <el-tag :type=\"getScoreType(getScore(row) / 100)\" effect=\"dark\">\r\n              {{ getScore(row) }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <el-pagination\r\n        background\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        :page-size=\"search.pageSize\"\r\n        :current-page=\"search.pageNumber\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :total=\"total\"\r\n        class=\"tech-pagination\"\r\n      >\r\n      </el-pagination>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getAdoptTaskStudent } from '../../../api/api'\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: false,\r\n      search: {\r\n        taskId: '',\r\n        userName: '',\r\n        pageNumber: 1,\r\n        pageSize: 10\r\n      },\r\n      total: 0,\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    getScoreType(percent) {\r\n      if (percent >= 0.85) return 'success'\r\n      if (percent >= 0.6) return 'warning'\r\n      return 'danger'\r\n    },\r\n    getScore(row) {\r\n      const taskScore =\r\n        ((row.videoCount + row.assignCount) / (row.assign + row.videoNum)) *\r\n        row.proportion\r\n      const testScore =\r\n        (row.testScore / row.totalScore) * (100 - row.proportion) || 0\r\n      return (taskScore + testScore).toFixed(2)\r\n    },\r\n    tableHeaderStyle() {\r\n      return {\r\n        color: '#4A2B90',\r\n        'background-color': '#ECE9F4',\r\n        'font-weight': 'bold',\r\n        'border-bottom': '1px solid #7B68EE'\r\n      }\r\n    },\r\n    tableRowStyle() {\r\n      return {\r\n        color: '#5F4B8B',\r\n        'font-size': '14px',\r\n        'border-bottom': '1px solid #F0EEF7'\r\n      }\r\n    },\r\n    searchPage() {\r\n      this.search.pageNumber = 1\r\n      this.query()\r\n    },\r\n    query() {\r\n      this.loading = true\r\n      // 这里taskId从路由取，确保参数完整\r\n      this.search.taskId = this.$route.query.id || this.search.taskId\r\n      getAdoptTaskStudent(this.search)\r\n        .then(res => {\r\n          if (res.code === 1000) {\r\n            this.tableData = res.data.records\r\n            this.total = res.data.total\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            })\r\n          }\r\n          this.loading = false\r\n        })\r\n        .catch(() => {\r\n          this.loading = false\r\n        })\r\n    },\r\n    refresh() {\r\n      this.search.userName = ''\r\n      this.search.pageNumber = 1\r\n      this.query()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.search.pageNumber = val\r\n      this.query()\r\n    },\r\n    handleSizeChange(val) {\r\n      this.search.pageSize = val\r\n      this.query()\r\n    }\r\n  },\r\n  mounted() {\r\n    this.query()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.score-management {\r\n  padding: 20px;\r\n  background-color: #F8F7FC;\r\n}\r\n\r\n.search-panel {\r\n  background: #fff;\r\n  border-radius: 8px;\r\n  padding: 15px;\r\n  margin-bottom: 20px;\r\n  box-shadow: 0 2px 12px rgba(123, 104, 238, 0.1);\r\n}\r\n\r\n.search-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.search-title {\r\n  font-size: 14px;\r\n  color: #4A2B90;\r\n  margin-bottom: 8px;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.search-title i {\r\n  margin-right: 6px;\r\n  color: #7B68EE;\r\n}\r\n\r\n.search-actions {\r\n  display: flex;\r\n  align-items: flex-end;\r\n  height: 100%;\r\n}\r\n\r\n.data-panel {\r\n  background: #fff;\r\n  border-radius: 8px;\r\n  padding: 15px;\r\n  box-shadow: 0 2px 12px rgba(123, 104, 238, 0.1);\r\n}\r\n\r\n.user-cell, .course-cell {\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.el-tag {\r\n  border-radius: 12px;\r\n  padding: 0 8px;\r\n  height: 24px;\r\n  line-height: 24px;\r\n  display: inline-flex;\r\n  align-items: center;\r\n}\r\n</style>\r\n\r\n<style>\r\n/* 全局样式 */\r\n.tech-input .el-input__inner {\r\n  border-radius: 20px;\r\n  border: 1px solid #D8D8E5;\r\n  color: #5F4B8B;\r\n  background-color: #F9F8FD;\r\n  padding-left: 30px;\r\n}\r\n\r\n.tech-input .el-input__prefix {\r\n  left: 5px;\r\n  color: #7B68EE;\r\n}\r\n\r\n.search-btn {\r\n  background: linear-gradient(135deg, #7B68EE, #9370DB);\r\n  border: none;\r\n  border-radius: 20px;\r\n  color: white;\r\n  padding: 7px 15px;\r\n}\r\n\r\n.reset-btn {\r\n  border-radius: 20px;\r\n  color: #7B68EE;\r\n  border: 1px solid #D8D8E5;\r\n  padding: 7px 15px;\r\n}\r\n\r\n.tech-table {\r\n  border-radius: 8px;\r\n  border: 1px solid #ECE9F4;\r\n}\r\n\r\n.tech-table .el-table__body tr:hover>td {\r\n  background-color: #F5F2FF !important;\r\n}\r\n\r\n.tech-pagination .el-pagination.is-background .el-pager li:not(.disabled).active {\r\n  background-color: #7B68EE;\r\n  color: white;\r\n  border-radius: 50%;\r\n}\r\n</style>"],"mappings":"AAiKA,SAAAA,mBAAA;AACA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,MAAA;QACAC,MAAA;QACAC,QAAA;QACAC,UAAA;QACAC,QAAA;MACA;MACAC,KAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,aAAAC,OAAA;MACA,IAAAA,OAAA;MACA,IAAAA,OAAA;MACA;IACA;IACAC,SAAAC,GAAA;MACA,MAAAC,SAAA,GACA,CAAAD,GAAA,CAAAE,UAAA,GAAAF,GAAA,CAAAG,WAAA,KAAAH,GAAA,CAAAI,MAAA,GAAAJ,GAAA,CAAAK,QAAA,IACAL,GAAA,CAAAM,UAAA;MACA,MAAAC,SAAA,GACAP,GAAA,CAAAO,SAAA,GAAAP,GAAA,CAAAQ,UAAA,UAAAR,GAAA,CAAAM,UAAA;MACA,QAAAL,SAAA,GAAAM,SAAA,EAAAE,OAAA;IACA;IACAC,iBAAA;MACA;QACAC,KAAA;QACA;QACA;QACA;MACA;IACA;IACAC,cAAA;MACA;QACAD,KAAA;QACA;QACA;MACA;IACA;IACAE,WAAA;MACA,KAAAxB,MAAA,CAAAG,UAAA;MACA,KAAAsB,KAAA;IACA;IACAA,MAAA;MACA,KAAA1B,OAAA;MACA;MACA,KAAAC,MAAA,CAAAC,MAAA,QAAAyB,MAAA,CAAAD,KAAA,CAAAE,EAAA,SAAA3B,MAAA,CAAAC,MAAA;MACAJ,mBAAA,MAAAG,MAAA,EACA4B,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAxB,SAAA,GAAAuB,GAAA,CAAA/B,IAAA,CAAAiC,OAAA;UACA,KAAA1B,KAAA,GAAAwB,GAAA,CAAA/B,IAAA,CAAAO,KAAA;QACA;UACA,KAAA2B,OAAA,CAAAC,KAAA;YACAC,KAAA;YACAC,OAAA,EAAAN,GAAA,CAAAM;UACA;QACA;QACA,KAAApC,OAAA;MACA,GACAqC,KAAA;QACA,KAAArC,OAAA;MACA;IACA;IACAsC,QAAA;MACA,KAAArC,MAAA,CAAAE,QAAA;MACA,KAAAF,MAAA,CAAAG,UAAA;MACA,KAAAsB,KAAA;IACA;IACAa,oBAAAC,GAAA;MACA,KAAAvC,MAAA,CAAAG,UAAA,GAAAoC,GAAA;MACA,KAAAd,KAAA;IACA;IACAe,iBAAAD,GAAA;MACA,KAAAvC,MAAA,CAAAI,QAAA,GAAAmC,GAAA;MACA,KAAAd,KAAA;IACA;EACA;EACAgB,QAAA;IACA,KAAAhB,KAAA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}