{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.some.js\";\nimport { mapState } from 'vuex';\nexport default {\n  name: \"asside\",\n  data() {\n    return {\n      isCollapse: false,\n      textColor: \"#A4B7CC\"\n    };\n  },\n  computed: {\n    ...mapState({\n      activeMenuArrary: state => state.menu.activeMenuArrary,\n      displayMenus: state => state.menu.displayMenus\n    }),\n    activeMenuKey() {\n      return this.$route.path + '@' + this.$route.name;\n    }\n  },\n  methods: {\n    handleSelect(key) {\n      const [path, name] = key.split(\"@\");\n      this.$router.push({\n        path\n      });\n      this.$store.commit('menu/setActiveMenu', path);\n      const exists = this.activeMenuArrary.some(item => item.url === path);\n      if (!exists) {\n        this.$store.commit('menu/addActiveMenu', {\n          name,\n          url: path\n        });\n      }\n    },\n    getMenuTypeOne(childArr) {\n      if (!Array.isArray(childArr)) return [];\n      return childArr.filter(one => one.menuType === 1);\n    }\n  },\n  mounted() {\n    this.$bus.$on('collapse', res => {\n      this.isCollapse = res;\n    });\n    this.$store.commit('menu/setActiveMenu', this.$route.path);\n    const exists = this.activeMenuArrary.some(item => item.url === this.$route.path);\n    if (!exists) {\n      this.$store.commit('menu/addActiveMenu', {\n        name: this.$route.name,\n        url: this.$route.path\n      });\n    }\n  }\n};","map":{"version":3,"names":["mapState","name","data","isCollapse","textColor","computed","activeMenuArrary","state","menu","displayMenus","activeMenuKey","$route","path","methods","handleSelect","key","split","$router","push","$store","commit","exists","some","item","url","getMenuTypeOne","childArr","Array","isArray","filter","one","menuType","mounted","$bus","$on","res"],"sources":["src/components/layout/aside.vue"],"sourcesContent":["<template>\r\n  <el-aside\r\n    :width=\"isCollapse ? '72px' : '240px'\"\r\n    style=\"background-color: #0F1220; border-right: 1px solid #252A3A\"\r\n  >\r\n    <div\r\n      class=\"back-icon\"\r\n      :style=\"{\r\n        width: isCollapse ? '71px' : '239px',\r\n        background: 'linear-gradient(90deg, #0F1220 0%, #1A1C3A 100%)',\r\n        borderBottom: '1px solid rgba(124, 77, 255, 0.1)'\r\n      }\"\r\n    >\r\n      <div class=\"logo-plain\">\r\n        <img\r\n          src=\"@/assets/image/systemlogo.png\"\r\n          :style=\"{\r\n            width: isCollapse ? '36px' : '180px',\r\n            height: 'auto',\r\n            transition: 'all 0.3s ease',\r\n            maxHeight: '50px'\r\n          }\"\r\n          alt=\"系统Logo\"\r\n        />\r\n      </div>\r\n    </div>\r\n    <el-menu\r\n      @select=\"handleSelect\"\r\n      :collapse=\"isCollapse\"\r\n      :collapse-transition=\"false\"\r\n      class=\"el-menu-vertical\"\r\n      :unique-opened=\"true\"\r\n      background-color=\"#0F1220\"\r\n      :text-color=\"textColor\"\r\n      :default-active=\"activeMenuKey\"\r\n      active-text-color=\"#ffffff\"\r\n    >\r\n      <span v-for=\"(item, index) in displayMenus\" :key=\"item.id || index\">\r\n        <!-- 菜单项 -->\r\n        <el-menu-item\r\n          v-if=\"item.menuType === 1\"\r\n          :index=\"item.routeUrl + '@' + item.menuName\"\r\n        >\r\n          <i :class=\"item.icon\" style=\"color: #5C6BC0;\"></i>\r\n          <span slot=\"title\">{{ item.menuName }}</span>\r\n        </el-menu-item>\r\n\r\n        <!-- 目录项 -->\r\n        <el-submenu v-else-if=\"item.menuType === 0\" :index=\"String(index)\">\r\n          <template #title>\r\n            <i :class=\"item.icon\" style=\"color: #5C6BC0;\"></i>\r\n            <span v-if=\"!isCollapse\">{{ item.menuName }}</span>\r\n          </template>\r\n\r\n          <span v-for=\"(element, subIndex) in item.child\" :key=\"element.id || subIndex\">\r\n            <el-submenu\r\n              v-if=\"element.menuType === 0\"\r\n              :index=\"index + '-' + subIndex\"\r\n            >\r\n              <template #title>\r\n                <i :class=\"element.icon\" style=\"color: #7986CB;\"></i>\r\n                <span>{{ element.menuName }}</span>\r\n              </template>\r\n              <el-menu-item\r\n                v-for=\"one in getMenuTypeOne(element.child)\"\r\n                :key=\"one.routeUrl + '@' + one.menuName\"\r\n                :index=\"one.routeUrl + '@' + one.menuName\"\r\n              >\r\n                <i :class=\"one.icon\" style=\"color: #9FA8DA;\"></i>\r\n                <span slot=\"title\">{{ one.menuName }}</span>\r\n              </el-menu-item>\r\n            </el-submenu>\r\n\r\n            <el-menu-item\r\n              v-else-if=\"element.menuType === 1\"\r\n              :index=\"element.routeUrl + '@' + element.menuName\"\r\n            >\r\n              <i :class=\"element.icon\" style=\"color: #9FA8DA;\"></i>\r\n              <span slot=\"title\">{{ element.menuName }}</span>\r\n            </el-menu-item>\r\n          </span>\r\n        </el-submenu>\r\n      </span>\r\n    </el-menu>\r\n  </el-aside>\r\n</template>\r\n\r\n<script>\r\nimport { mapState } from 'vuex'\r\n\r\nexport default {\r\n  name: \"asside\",\r\n  data() {\r\n    return {\r\n      isCollapse: false,\r\n      textColor: \"#A4B7CC\"\r\n    }\r\n  },\r\n  computed: {\r\n    ...mapState({\r\n      activeMenuArrary: state => state.menu.activeMenuArrary,\r\n      displayMenus: state => state.menu.displayMenus\r\n    }),\r\n    activeMenuKey() {\r\n      return this.$route.path + '@' + this.$route.name;\r\n    }\r\n  },\r\n  methods: {\r\n    handleSelect(key) {\r\n      const [path, name] = key.split(\"@\");\r\n      this.$router.push({ path });\r\n      this.$store.commit('menu/setActiveMenu', path);\r\n\r\n      const exists = this.activeMenuArrary.some(item => item.url === path);\r\n      if (!exists) {\r\n        this.$store.commit('menu/addActiveMenu', { name, url: path });\r\n      }\r\n    },\r\n    getMenuTypeOne(childArr) {\r\n      if (!Array.isArray(childArr)) return [];\r\n      return childArr.filter(one => one.menuType === 1);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.$bus.$on('collapse', res => {\r\n      this.isCollapse = res;\r\n    });\r\n\r\n    this.$store.commit('menu/setActiveMenu', this.$route.path);\r\n\r\n    const exists = this.activeMenuArrary.some(item => item.url === this.$route.path);\r\n    if (!exists) {\r\n      this.$store.commit('menu/addActiveMenu', { name: this.$route.name, url: this.$route.path });\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n\r\n<style scoped lang=\"scss\">\r\n    .logo-plain {\r\n  height: 100%;\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  padding: 0 16px;\r\n  \r\n  img {\r\n    object-fit: contain;\r\n    display: block;\r\n  }\r\n}\r\n    \r\n    .back-icon {\r\n        display: flex;\r\n        height: 80px;\r\n        justify-content: center;\r\n        align-items: center;\r\n        transition: all 0.3s;\r\n        border-bottom: 1px solid rgba(124, 77, 255, 0.1);\r\n        \r\n        .logo-text {\r\n            transition: all 0.3s;\r\n            span {\r\n                transition: all 0.3s;\r\n                &:hover {\r\n                    background: rgba(124, 77, 255, 0.3) !important;\r\n                }\r\n            }\r\n        }\r\n    }\r\n    \r\n    .el-menu {\r\n        background: transparent !important;\r\n        border-right: none !important;\r\n        \r\n        &:not(.el-menu--collapse) {\r\n            width: 240px;\r\n        }\r\n        \r\n        /* 菜单项基础样式 */\r\n        &:deep(.el-menu-item),\r\n        &:deep(.el-submenu__title) {\r\n            color: #C5CAE9;\r\n            height: 48px !important;\r\n            line-height: 48px !important;\r\n            margin: 4px 12px;\r\n            border-radius: 6px;\r\n            font-size: 14px;\r\n            transition: all 0.2s;\r\n            \r\n            i {\r\n                font-size: 18px;\r\n                margin-right: 10px;\r\n            }\r\n            \r\n            /* 悬停效果 */\r\n            &:hover {\r\n                background: rgba(94, 53, 177, 0.2) !important;\r\n                color: #E1E1FF;\r\n                \r\n                i {\r\n                    color: #B388FF;\r\n                }\r\n            }\r\n        }\r\n        \r\n        /* 激活状态 - 保留原来的白色高亮条 */\r\n        &:deep(.el-menu-item.is-active) {\r\n             background: linear-gradient(90deg, rgba(124, 77, 255, 0.15), transparent) !important;\r\n            color: #FFFFFF !important;\r\n            font-weight: 500;\r\n            position: relative;\r\n            \r\n            i {\r\n                color: #7C4DFF !important;\r\n            }\r\n            \r\n            &::after {\r\n                    content: '';\r\n                    position: absolute;\r\n                    left: 0;\r\n                    top: 0;\r\n                    height: 100%;\r\n                    width: 3px;\r\n                    background: linear-gradient(to bottom, #7E57C2, #5C6BC0);\r\n            } \r\n                           \r\n            &::before {\r\n                content: '';\r\n                position: absolute;\r\n                left: 0;\r\n                top: 0;\r\n                height: 100%;\r\n                width: 3px;\r\n                background: #ffffff;\r\n                border-radius: 0 3px 3px 0;\r\n            }\r\n        }\r\n        \r\n        /* 子菜单样式 */\r\n        &:deep(.el-submenu) {\r\n            .el-submenu__title {\r\n                color: #C5CAE9;\r\n            }\r\n            \r\n            .el-menu {\r\n                background: rgba(15, 18, 32, 0.8) !important;\r\n                \r\n                .el-menu-item {\r\n                    padding-left: 54px !important;\r\n                    color: #A4B7CC;\r\n                    \r\n                    i {\r\n                        color: #7986CB;\r\n                    }\r\n                }\r\n            }\r\n        }\r\n    }\r\n    \r\n    /* 折叠状态下的微调 */\r\n    .el-menu--collapse {\r\n        .el-submenu {\r\n            & > .el-submenu__title {\r\n                padding-left: 20px !important;\r\n                \r\n                .el-submenu__icon-arrow {\r\n                    display: none;\r\n                }\r\n            }\r\n        }\r\n    }\r\n    \r\n    .xiaoshi {\r\n        opacity: 0;\r\n        width: 0;\r\n        height: 0;\r\n        overflow: hidden;\r\n    }\r\n</style>"],"mappings":";;;;AAwFA,SAAAA,QAAA;AAEA;EACAC,IAAA;EACAC,KAAA;IACA;MACAC,UAAA;MACAC,SAAA;IACA;EACA;EACAC,QAAA;IACA,GAAAL,QAAA;MACAM,gBAAA,EAAAC,KAAA,IAAAA,KAAA,CAAAC,IAAA,CAAAF,gBAAA;MACAG,YAAA,EAAAF,KAAA,IAAAA,KAAA,CAAAC,IAAA,CAAAC;IACA;IACAC,cAAA;MACA,YAAAC,MAAA,CAAAC,IAAA,cAAAD,MAAA,CAAAV,IAAA;IACA;EACA;EACAY,OAAA;IACAC,aAAAC,GAAA;MACA,OAAAH,IAAA,EAAAX,IAAA,IAAAc,GAAA,CAAAC,KAAA;MACA,KAAAC,OAAA,CAAAC,IAAA;QAAAN;MAAA;MACA,KAAAO,MAAA,CAAAC,MAAA,uBAAAR,IAAA;MAEA,MAAAS,MAAA,QAAAf,gBAAA,CAAAgB,IAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,GAAA,KAAAZ,IAAA;MACA,KAAAS,MAAA;QACA,KAAAF,MAAA,CAAAC,MAAA;UAAAnB,IAAA;UAAAuB,GAAA,EAAAZ;QAAA;MACA;IACA;IACAa,eAAAC,QAAA;MACA,KAAAC,KAAA,CAAAC,OAAA,CAAAF,QAAA;MACA,OAAAA,QAAA,CAAAG,MAAA,CAAAC,GAAA,IAAAA,GAAA,CAAAC,QAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAC,IAAA,CAAAC,GAAA,aAAAC,GAAA;MACA,KAAAhC,UAAA,GAAAgC,GAAA;IACA;IAEA,KAAAhB,MAAA,CAAAC,MAAA,4BAAAT,MAAA,CAAAC,IAAA;IAEA,MAAAS,MAAA,QAAAf,gBAAA,CAAAgB,IAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,GAAA,UAAAb,MAAA,CAAAC,IAAA;IACA,KAAAS,MAAA;MACA,KAAAF,MAAA,CAAAC,MAAA;QAAAnB,IAAA,OAAAU,MAAA,CAAAV,IAAA;QAAAuB,GAAA,OAAAb,MAAA,CAAAC;MAAA;IACA;EACA;AACA","ignoreList":[]},"metadata":{},"sourceType":"module","externalDependencies":[]}