1 ? 'warning' : 'info'\"\n              class=\"status-tag\">\n              {{ getStatusText(row.state) }}\n            </el-tag>\n          </template>\n        </el-table-column>\n        <el-table-column prop=\"createBy\" label=\"创建者\" width=\"100\"></el-table-column>\n        <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"150\"></el-table-column>\n        <el-table-column label=\"操作\" width=\"400\">\n          <template #default=\"{row}\">\n            <div class=\"action-buttons\">\n              <el-button \n                size=\"mini\" \n                v-if=\"row.state == 0\" \n                @click=\"offShelf(row.id)\"\n                class=\"action-btn reject-btn\">\n                <i class=\"el-icon-close\"></i> 审核未通过\n              </el-button>\n              <el-button \n                size=\"mini\" \n                v-if=\"row.state == 2 || row.state == 1\" \n                @click=\"listing(row.id)\"\n                class=\"action-btn approve-btn\">\n                <i class=\"el-icon-check\"></i> 审核通过\n              </el-button>\n              <el-button \n                size=\"mini\" \n                @click=\"toArticleComment(row.id)\"\n                class=\"action-btn comment-btn\">\n                评论\n              </el-button>\n              <el-button \n                size=\"mini\" \n                @click=\"updateData(row.id)\"\n                class=\"action-btn edit-btn\">\n                修改\n              </el-button>\n              <el-popconfirm\n                title=\"确认删除选中的数据？\"\n                @confirm=\"deleteDate(row.id)\">\n                <el-button \n                  size=\"mini\" \n                  slot=\"reference\"\n                  class=\"action-btn delete-btn\">\n                  删除\n                </el-button>\n              </el-popconfirm>\n            </div>\n          </template>\n        </el-table-column>\n      </el-table>\n\n      <el-pagination\n        background\n        layout=\