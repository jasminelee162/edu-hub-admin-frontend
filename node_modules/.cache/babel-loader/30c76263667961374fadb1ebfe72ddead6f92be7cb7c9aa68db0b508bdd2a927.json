=\"8\" :xl=\"8\">\r\n                    <el-button style=\"margin-top:10px\" size=\"mini\" icon=\"el-icon-search\" type=\"primary\" @click=\"searchPage\">查询</el-button>\r\n                    <el-button style=\"margin-top:10px\" size=\"mini\" icon=\"el-icon-refresh\" @click=\"refresh\">重置</el-button>\r\n                </el-col>\r\n            </el-row>\r\n        </div>\r\n        <div class=\"table\">\r\n            <el-row style=\"padding-top:10px;margin-left:10px\">\r\n            </el-row>\r\n            <el-table\r\n            v-loading=\"loading\"\r\n            :data=\"tableData\"\r\n            :header-cell-style=\"{\r\n              'color': '#4A2B90',\r\n              'background-color': '#ECE9F4',\r\n            }\"\r\n            :row-style=\"{\r\n              'color': '#888897',\r\n              'font-size': '15px',\r\n              'font-family':'黑体',\r\n              'white-space': 'nowrap'\r\n            }\"\r\n            stripe\r\n            style=\"width: 100%\">\r\n            <el-table-column\r\n              type=\"selection\"\r\n              width=\"55\">\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"createBy\"\r\n              label=\"学生姓名\"\r\n              >\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"testName\"\r\n              label=\"考试名称\"\r\n              >\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"totalScore\"\r\n              label=\"总分\"\r\n              >\r\n            </el-table-column>\r\n            <el-table-column\r\n              prop=\"totalGetScore\"\r\n              label=\"得分\"\r\n              >\r\n            </el-table-column>\r\n            <el-table-column\r\n              label=\"操作\"\r\n              >\r\n              <template slot-scope=\"scope\">\r\n                <el-button size=\"mini\" type=\"warning\" @click=\"yuejuan(scope.row.userId)\">阅卷</el-button>\r\n        