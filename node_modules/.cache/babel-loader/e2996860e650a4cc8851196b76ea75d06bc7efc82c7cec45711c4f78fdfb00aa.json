ry\" @click=\"submit\">确 定</el-button>\r\n      <el-button size=\"mini\" @click=\"handleClose\">取 消</el-button>\r\n    </div>\r\n  </el-dialog>\r\n</div>\r\n</template>\r\n\r\n<script>\r\n  import {editForum,getForumById} from '../../../api/api'\r\n  export default {\r\n    data() {\r\n      return{\r\n        formLabelWidth: '80px',\r\n        form: {\r\n          name: \"\",\r\n          content: \"\",\r\n          remark: \"\",\r\n        },\r\n        rules: {\r\n          name: [\r\n            { required: true, message: '请输入讨论名称', trigger: 'blur' },\r\n          ],\r\n          content: [\r\n            { required: true, message: '请输入讨论内容', trigger: 'blur' },\r\n          ],\r\n        },\r\n      }\r\n    },\r\n    props: ['updateVisible','updateId'],\r\n    methods: {\r\n      submit() {\r\n        this.$refs[\"ruleForm\"].validate((valid) => {\r\n          if (valid) {\r\n            editForum(this.form).then(res => {\r\n              if(res.code == 1000) {\r\n                this.$notify.success({\r\n                  title: '成功',\r\n                  message: \"保存成功\"\r\n                });\r\n                this.handleClose();\r\n              } else {\r\n                this.$notify.error({\r\n                  title: '错误',\r\n                  message: res.message\r\n                });\r\n              }\r\n            })\r\n          } else {\r\n            return false;\r\n          }\r\n        });\r\n      },\r\n      handleClose() {\r\n        this.$emit(\"updateFalse\")\r\n      },\r\n     \r\n    },\r\n    created() {\r\n     \r\n    },\r\n    mounted() {\r\n      \r\n    },\r\n    watch: {\r\n      updateId(newVal) {\r\n        if(newVal) {\r\n          getForumById({id:newVal}).then(res => {\r\n            if(res.code == 1000) {\r\n              this.form = res.data\r\n            } else {\r\n              this.$notify.error({\r\n                title: '错误',\r\n                message: res.message\r\n              });\r\n            }\r\n   