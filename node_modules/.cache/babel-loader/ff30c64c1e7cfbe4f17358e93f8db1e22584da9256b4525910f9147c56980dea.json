 this.chapter = res.data;\n        this.search.taskId = res.data.taskId;\n        this.query();\n      } else {\n        this.$notify.error({\n          title: '错误',\n          message: res.message || '获取章节信息失败'\n        });\n      }\n    }).catch(error => {\n      console.error(\"获取章节信息出错:\", error);\n      this.$notify.error({\n        title: '错误',\n        message: '获取章节信息过程中发生错误'\n      });\n    });\n  }\n}\n</script>\n\n<style scoped>\n.chapter-student-management {\n  padding: 20px;\n  background-color: #F8F7FC;\n}\n\n.search-panel {\n  background: #fff;\n  border-radius: 8px;\n  padding: 15px;\n  margin-bottom: 20px;\n  box-shadow: 0 2px 12px rgba(123, 104, 238, 0.1);\n}\n\n.search-item {\n  display: flex;\n  flex-direction: column;\n}\n\n.search-title {\n  font-size: 14px;\n  color: #4A2B90;\n  margin-bottom: 8px;\n  display: flex;\n  align-items: center;\n}\n\n.search-title i {\n  margin-right: 6px;\n  color: #7B68EE;\n}\n\n.search-actions {\n  display: flex;\n  align-items: flex-end;\n  height: 100%;\n}\n\n.data-panel {\n  background: #fff;\n  border-radius: 8px;\n  padding: 15px;\n  box-shadow: 0 2px 12px rgba(123, 104, 238, 0.1);\n}\n\n.user-cell, .course-cell, .chapter-cell {\n  display: flex;\n  align-items: center;\n}\n\n.el-tag {\n  border-radius: 12px;\n  padding: 0 8px;\n  height: 24px;\n  line-height: 24px;\n  display: inline-flex;\n  align-items: center;\n}\n</style>\n\n<style>\n/* 全局样式 */\n.tech-input .el-input__inner {\n  border-radius: 20px;\n  border: 1px solid #D8D8E5;\n  color: #5F4B8B;\n  background-color: #F9F8FD;\n  padding-left: 30px;\n}\n\n.tech-input .el-input__prefix {\n  left: 5px;\n  color: #7B68EE;\n}\n\n.search-btn {\n  background: linear-gradient(135deg, #7B68EE, #9370DB);\n  border: none;\n  border-radius: 20px;\n  color: white;\n  padding: 7px 15px;\n}\n\n.reset-btn {\n  border-radius: 20px;\n  color: #7B68EE;\n  border: 1px solid #D8D8E5;\n  padding: 7px 15px;\n}\n\n.tech-table {\n  border-radius: 8px;\n  border: 1px solid #ECE9F4;\n}\n\n.tech-table .el-table__body tr:hover>td {\n  background-color: #F5F2FF !important;\n}\n\n.tech-pagination .el-pagination.is-background .el-pager li:not(.disabled).active {\n  background-color: #7B68EE;\n  color: white;\n  border-radius: 50%;\n}\n</style>"],"mappings":";;AA+HA,SAAAA,qBAAA,EAAAC,cAAA,EAAAC,sBAAA,EAAAC,kBAAA;AAEA;EACAC,KAAA;IACA;MACAC,OAAA;MACAC,MAAA;MACAC,MAAA;MACAC,QAAA;MACAC,UAAA;MACAC,aAAA;MACAC,SAAA;MACAC,OAAA;MACAC,MAAA;QACAF,SAAA;QACAG,MAAA;QACAC,QAAA;QACAC,KAAA;QACAC,UAAA;QACAC,QAAA;MACA;MACAC,KAAA;MACAC,SAAA;IACA;EACA;EACAC,OAAA;IACAC,iBAAA;MACA;QACA;QACA;QACA;QACA;MACA;IACA;IACAC,cAAA;MACA;QACA;QACA;QACA;MACA;IACA;IACAC,WAAA;MACA,KAAAX,MAAA,CAAAI,UAAA;MACA,KAAAQ,KAAA;IACA;IACAA,MAAA;MACA,KAAApB,OAAA;MACAF,kBAAA,MAAAU,MAAA,EAAAa,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAAR,SAAA,GAAAO,GAAA,CAAAvB,IAAA,CAAAyB,OAAA;UACA,KAAAV,KAAA,GAAAQ,GAAA,CAAAvB,IAAA,CAAAe,KAAA;QACA;UACA,KAAAW,OAAA,CAAAC,KAAA;YACAC,KAAA;YACAC,OAAA,EAAAN,GAAA,CAAAM;UACA;QACA;MACA,GAAAC,KAAA,CAAAH,KAAA;QACAI,OAAA,CAAAJ,KAAA,UAAAA,KAAA;QACA,KAAAD,OAAA,CAAAC,KAAA;UACAC,KAAA;UACAC,OAAA;QACA;MACA,GAAAG,OAAA;QACA,KAAA/B,OAAA;MACA;IACA;IACAgC,QAAA;MACA,KAAAxB,MAAA,CAAAE,QAAA;MACA,KAAAF,MAAA,CAAAG,KAAA;MACA,KAAAQ,UAAA;IACA;IACAc,oBAAAC,GAAA;MACA,KAAA1B,MAAA,CAAAI,UAAA,GAAAsB,GAAA;MACA,KAAAd,KAAA;IACA;IACAe,iBAAAD,GAAA;MACA,KAAA1B,MAAA,CAAAK,QAAA,GAAAqB,GAAA;MACA,KAAAd,KAAA;IACA;IACAgB,sBAAAF,GAAA;MACA,KAAAjC,MAAA,GAAAiC,GAAA,CAAAG,GAAA,CAAAC,IAAA,IAAAA,IAAA,CAAAC,EAAA;MACA,KAAArC,MAAA,YAAAD,MAAA;IACA;EACA;EACAuC,QAAA;IACA,KAAAlC,SAAA,QAAAmC,MAAA,CAAArB,KAAA,CAAAmB,EAAA;IACA,KAAA/B,MAAA,CAAAF,SAAA,QAAAmC,MAAA,CAAArB,KAAA,CAAAmB,EAAA;IAEA3C,cAAA;MAAA2C,EAAA,OAAAjC;IAAA,GAAAe,IAAA,CAAAC,GAAA;MACA,IAAAA,GAAA,CAAAC,IAAA;QACA,KAAAhB,OAAA,GAAAe,GAAA,CAAAvB,IAAA;QACA,KAAAS,MAAA,CAAAC,MAAA,GAAAa,GAAA,CAAAvB,IAAA,CAAAU,MAAA;QACA,KAAAW,KAAA;MACA;QACA,KAAAK,OAAA,CAAAC,KAAA;UACAC,KAAA;UACAC,OAAA,EAAAN,GAAA,CAAAM,OAAA;QACA;MACA;IACA,GAAAC,KAAA,CAAAH,KAAA;MACAI,OAAA,CAAAJ,KAAA,cA