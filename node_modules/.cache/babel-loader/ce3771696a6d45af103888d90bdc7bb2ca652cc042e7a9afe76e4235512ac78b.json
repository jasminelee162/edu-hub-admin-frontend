us + \"\";\n            if (this.form.idArrary != \"0\") {\n              this.parentId = this.form.idArrary.split(\",\");\n            }\n          } else {\n            this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }\n    }\n  }\n};","map":{"version":3,"names":["getUserById","editUser","data","formLabelWidth","form","idArrary","deptId","userName","loginAccount","userType","email","tel","sex","avatar","password","status","remark","roleIds","parentId","rules","required","message","trigger","props","methods","submit","then","res","code","$notify","success","title","handleClose","error","handleChange","value","length","join","$emit","watch","updateId","newVal","id","split"],"sources":["src/components/system/user/updateUser.vue"],"sourcesContent":["<template>\n<div>\n  <el-dialog title=\"修改用户\" width=\"40%\" :destroy-on-close=\"true\" :visible.sync=\"updateUserVisible\" :before-close=\"handleClose\">\n    <el-form :model=\"form\" :rules=\"rules\" ref=\"ruleForm\">\n      <el-row :gutter=\"10\">\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\n            <span class=\"search-title\">用户昵称:</span>\n            <el-form-item prop=\"userName\" style=\"margin-bottom:0\">\n              <el-input v-model=\"form.userName\" size=\"mini\" placeholder=\"请输入用户昵称\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n        </el-col>\n        \n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\n            <span class=\"search-title\">手机号码:</span>\n            <el-form-item prop=\"tel\" style=\"margin-bottom:0\">\n              <el-input v-model=\"form.tel\" size=\"mini\" placeholder=\"请输入手机号码\" autocomplete=\"off\"></el-input>\n            </el-form-item>\n        </el-col>\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"12\" :lg=\"12\" :xl=\"12\">\n            <span class=\"search-title\">&nbsp;&nbsp;&nbsp;&nbsp;邮箱:</span>\n            <