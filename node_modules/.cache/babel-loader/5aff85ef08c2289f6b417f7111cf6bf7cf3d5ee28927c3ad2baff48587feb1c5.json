{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getDeptList, removeDept } from '../../../api/api';\nimport addDept from '../../../components/system/dept/addDept';\nimport updateDept from '../../../components/system/dept/updateDept';\nexport default {\n  data() {\n    return {\n      loading: true,\n      refreshTable: true,\n      expand: true,\n      addDeptVisible: false,\n      updateDeptVisible: false,\n      search: {\n        deptName: \"\",\n        status: \"\"\n      },\n      tableData: [],\n      parentId: [],\n      disable: false,\n      updateId: \"\"\n    };\n  },\n  components: {\n    addDept,\n    updateDept\n  },\n  methods: {\n    // 搜索\n    query() {\n      getDeptList(this.search).then(res => {\n        if (res.code == 1000) {\n          this.tableData = res.data;\n          this.loading = false;\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    // 重置\n    refresh() {\n      this.search.deptName = \"\";\n      this.search.status = \"\";\n      this.query();\n    },\n    changeExpand() {\n      this.refreshTable = false;\n      this.expand = !this.expand;\n      this.$nextTick(() => {\n        this.refreshTable = true;\n      });\n    },\n    addDept() {\n      this.addDeptVisible = true;\n    },\n    addDeptFalse() {\n      this.addDeptVisible = false;\n      this.query();\n      this.parentId = [];\n      this.disable = false;\n    },\n    updateDept() {\n      this.updateDeptVisible = true;\n    },\n    updateDeptFalse() {\n      this.updateId = \"\";\n      this.updateDeptVisible = false;\n      this.query();\n    },\n    handleCommand(command) {\n      var data = command.split(\"#\");\n      if (data[1] == 'add') {\n        var idArrary = data[0].split(\",\");\n        var id = data[2];\n        if (idArrary[0] != \"0\") {\n          this.parentId = idArrary;\n        }\n        this.parentId.push(id);\n        this.disable = true;\n        this.addDept();\n      } else if (data[1] == 'edit') {\n        this.updateId = data[0];\n        this.updateDept();\n      } else {\n        this.deleteDate(data[0]);\n      }\n    },\n    deleteDate(id) {\n      this.$confirm('确定删除选中的数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning'\n      }).then(() => {\n        removeDept({\n          id: id\n        }).then(res => {\n          if (res.code == 1000) {\n            this.$message({\n              type: 'success',\n              message: '删除成功!'\n            });\n            this.pageNumber = 1;\n            this.query();\n          } else {\n            this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }).catch(() => {});\n    }\n  },\n  mounted() {\n    //初始化页面调用查询方法\n    this.query();\n  }\n};","map":{"version":3,"names":["getDeptList","removeDept","addDept","updateDept","data","loading","refreshTable","expand","addDeptVisible","updateDeptVisible","search","deptName","status","tableData","parentId","disable","updateId","components","methods","query","then","res","code","$notify","error","title","message","refresh","changeExpand","$nextTick","addDeptFalse","updateDeptFalse","handleCommand","command","split","idArrary","id","push","deleteDate","$confirm","confirmButtonText","cancelButtonText","type","$message","pageNumber","catch","mounted"],"sources":["src/views/system/dept/dept.vue"],"sourcesContent":["<template>\n  <div class=\"dept\">\n      <div class=\"search-table\">\n        <div class=\"search\">\n            <el-row :gutter=\"10\" style=\"padding:10px\">\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\n                    <span class=\"search-title\">部门名称:</span>\n                    <el-input\n                        style=\"margin-top:10px\"\n                        size=\"mini\"\n                  