{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.array-buffer.detached.js\";\nimport \"core-js/modules/es.array-buffer.transfer.js\";\nimport \"core-js/modules/es.array-buffer.transfer-to-fixed-length.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.find.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.typed-array.to-reversed.js\";\nimport \"core-js/modules/es.typed-array.to-sorted.js\";\nimport \"core-js/modules/es.typed-array.with.js\";\nimport \"core-js/modules/web.dom-exception.stack.js\";\nimport \"core-js/modules/web.url-search-params.delete.js\";\nimport \"core-js/modules/web.url-search-params.has.js\";\nimport \"core-js/modules/web.url-search-params.size.js\";\nimport { uploadTemplate, getTemplates, deleteTemplate, getTemplateContent } from '../../../api/api';\nimport { renderAsync } from 'docx-preview';\nexport default {\n  data() {\n    return {\n      loading: true,\n      selected: [],\n      search: {\n        name: \"\",\n        dateRange: [],\n        pageNumber: 1,\n        pageSize: 10\n      },\n      total: 0,\n      tableData: []\n    };\n  },\n  methods: {\n    // Ë°®Ê†ºÂ§¥ÈÉ®Ê†∑Âºè\n    tableHeaderStyle() {\n      return {\n        'color': '#4A2B90',\n        'background-color': '#ECE9F4',\n        'font-weight': 'bold',\n        'border-bottom': '1px solid #7B68EE'\n      };\n    },\n    // Ë°®Ê†ºË°åÊ†∑Âºè\n    tableRowStyle() {\n      return {\n        'color': '#5F4B8B',\n        'font-size': '14px',\n        'border-bottom': '1px solid #F0EEF7'\n      };\n    },\n    // ‰∏ä‰º†ÂâçÊ†°È™å\n    beforeUpload(file) {\n      const isLt10M = file.size / 1024 / 1024 < 10;\n      if (!isLt10M) {\n        this.$message.error('‰∏ä‰º†Êñá‰ª∂Â§ßÂ∞è‰∏çËÉΩË∂ÖËøá10MB!');\n        return false;\n      }\n\n      // Ê£ÄÊü•Êñá‰ª∂Á±ªÂûã\n      const allowedTypes = ['application/pdf', 'application/msword', 'application/vnd.openxmlformats-officedocument.wordprocessingml.document', 'application/vnd.ms-excel', 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet', 'application/vnd.ms-powerpoint', 'application/vnd.openxmlformats-officedocument.presentationml.presentation', 'image/jpeg', 'image/png'];\n      if (!allowedTypes.includes(file.type)) {\n        this.$message.error('Âè™ÊîØÊåÅ‰∏ä‰º†PDF„ÄÅWord„ÄÅExcel„ÄÅPPT„ÄÅJPGÂíåPNGÊñá‰ª∂!');\n        return false;\n      }\n      return true;\n    },\n    // Êñá‰ª∂‰∏ä‰º†ÊñπÊ≥ïÔºàÂÆåÊï¥ÂÆûÁé∞Ôºâ\n    uploadFile({\n      file,\n      onProgress,\n      onSuccess,\n      onError\n    }) {\n      const formData = new FormData();\n      formData.append('file', file); // ‚úÖ ÂÖ≥ÈîÆÁÇπÔºöÂøÖÈ°ªÊòØ file\n\n      uploadTemplate(formData, {\n        // headers: { 'Content-Type': 'multipart/form-data' },\n        onUploadProgress: progressEvent => {\n          const percentCompleted = Math.round(progressEvent.loaded * 100 / progressEvent.total);\n          onProgress({\n            percent: percentCompleted\n          }); // ÊîØÊåÅËøõÂ∫¶Êù°\n        }\n      }).then(res => {\n        if (res.code === 1000) {\n          this.$notify.success({\n            title: 'ÊàêÂäü',\n            message: '‰∏ä‰º†ÊàêÂäü'\n          });\n          this.query();\n          onSuccess(res); // ‚úÖ ÂøÖÈ°ªÂõûË∞ÉÊàêÂäü\n        } else {\n          this.$notify.error({\n            title: 'ÈîôËØØ',\n            message: res.message || '‰∏ä‰º†Â§±Ë¥•'\n          });\n          onError(new Error(res.message || '‰∏ä‰º†Â§±Ë¥•')); // ‚úÖ ÂõûË∞ÉÂ§±Ë¥•\n        }\n      }).catch(error => {\n        this.$notify.error({\n          title: 'ÈîôËØØ',\n          message: error.message || '‰∏ä‰º†Â§±Ë¥•'\n        });\n        onError(error); // ‚úÖ ÂõûË∞ÉÂ§±Ë¥•\n      });\n    },\n    // Êü•ÁúãÂÜÖÂÆπ Âú®Á∫øÈ¢ÑËßàdocx ÊàëÊé•‰∫Ü‰∏Ä‰∏™Ë∞∑Ê≠åÁöÑÈìæÊé•\n    viewContent(id) {\n      this.loading = true;\n      getTemplateContent({\n        id\n      }).then(res => {\n        this.loading = false;\n        if (res.code === 1000) {\n          const template = this.tableData.find(item => item.id === id);\n          const data = res.data;\n          const fileType = template.fileType?.toLowerCase();\n          if (!data.fileContent) {\n            this.$message.error('Êú™Ëé∑ÂèñÂà∞Êñá‰ª∂ÂÜÖÂÆπ');\n            return;\n          }\n          const mimeType = this.getMimeType(fileType);\n          const blob = this.base64ToBlob(data.fileContent, mimeType);\n          const fileURL = URL.createObjectURL(blob);\n          if (fileType === 'pdf') {\n            window.open(fileURL, '_blank');\n          } else if (['jpg', 'jpeg', 'png'].includes(fileType)) {\n            const imgWindow = window.open('', '_blank');\n            imgWindow.document.write(`<img src=\"${fileURL}\" style=\"max-width:100%\"/>`);\n          } else if (fileType === 'docx') {\n            this.showDocx(data.fileContent); // üëà ‰ΩøÁî® docx-preview Ê∏≤Êüì\n          } else {\n            this.$alert('ËØ•Ê†ºÂºèÊöÇ‰∏çÊîØÊåÅÂú®Á∫øÈ¢ÑËßàÔºåËØ∑‰∏ãËΩΩÊü•Áúã', 'ÊèêÁ§∫');\n          }\n        } else {\n          this.$notify.error({\n            title: 'ÈîôËØØ',\n            message: res.message || 'Ëé∑ÂèñÂÜÖÂÆπÂ§±Ë¥•'\n          });\n        }\n      }).catch(error => {\n        this.loading = false;\n        this.$notify.error({\n          title: 'ÈîôËØØ',\n          message: error.message || 'Ëé∑ÂèñÂÜÖÂÆπÂ§±Ë¥•'\n        });\n      });\n    },\n    //Êñá‰ª∂‰∫åËøõÂà∂ËΩ¨Âåñ\n    base64ToBlob(base64Data, mimeType = '') {\n      const byteCharacters = atob(base64Data);\n      const byteArrays = [];\n      for (let offset = 0; offset < byteCharacters.length; offset += 512) {\n        const slice = byteCharacters.slice(offset, offset + 512);\n        const byteNumbers = new Array(slice.length);\n        for (let i = 0; i < slice.length; i++) {\n          byteNumbers[i] = slice.charCodeAt(i);\n        }\n        byteArrays.push(new Uint8Array(byteNumbers));\n      }\n      return new Blob(byteArrays, {\n        type: mimeType\n      });\n    },\n    getMimeType(fileType) {\n      const map = {\n        pdf: 'application/pdf',\n        jpg: 'image/jpeg',\n        jpeg: 'image/jpeg',\n        png: 'image/png',\n        txt: 'text/plain',\n        doc: 'application/msword',\n        docx: 'application/vnd.openxmlformats-officedocument.wordprocessingml.document',\n        ppt: 'application/vnd.ms-powerpoint',\n        pptx: 'application/vnd.openxmlformats-officedocument.presentationml.presentation',\n        xls: 'application/vnd.ms-excel',\n        xlsx: 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet'\n      };\n      return map[fileType] || 'application/octet-stream';\n    },\n    //pptx excel docxÈ¢ÑËßà\n    showDocx(fileContentBase64) {\n      const blob = this.base64ToBlob(fileContentBase64, 'application/vnd.openxmlformats-officedocument.wordprocessingml.document');\n      const container = document.createElement('div');\n      container.style = 'padding:20px;max-height:90vh;overflow:auto;';\n      renderAsync(blob, container).then(() => {\n        const win = window.open('', '_blank');\n        win.document.body.appendChild(container);\n      });\n    },\n    // ‰∏ãËΩΩÊñá‰ª∂\n    downloadFile(id) {\n      this.loading = true;\n      getTemplateContent({\n        id\n      }).then(res => {\n        this.loading = false;\n        if (res.code === 1000) {\n          const template = this.tableData.find(item => item.id === id);\n          const base64 = res.data.fileContent;\n          const mimeType = this.getMimeType(template.fileType);\n          const blob = this.base64ToBlob(base64, mimeType);\n          const url = URL.createObjectURL(blob);\n          const link = document.createElement('a');\n          link.href = url;\n          link.download = template.name || 'download';\n          document.body.appendChild(link);\n          link.click();\n          document.body.removeChild(link);\n          URL.revokeObjectURL(url);\n          this.$notify.success({\n            title: 'ÊàêÂäü',\n            message: 'Êñá‰ª∂‰∏ãËΩΩÂ∑≤ÂºÄÂßã',\n            duration: 2000\n          });\n        } else {\n          this.$notify.error({\n            title: 'ÈîôËØØ',\n            message: res.message || '‰∏ãËΩΩÂ§±Ë¥•'\n          });\n        }\n      }).catch(error => {\n        this.loading = false;\n        this.$notify.error({\n          title: 'ÈîôËØØ',\n          message: error.message || '‰∏ãËΩΩÂ§±Ë¥•'\n        });\n      });\n    },\n    // ÊêúÁ¥¢\n    searchPage() {\n      this.search.pageNumber = 1;\n      this.query();\n    },\n    // Êü•ËØ¢Êï∞ÊçÆ\n    query() {\n      this.loading = true;\n      const params = {\n        ...this.search,\n        startDate: this.search.dateRange ? this.search.dateRange[0] : '',\n        endDate: this.search.dateRange ? this.search.dateRange[1] : ''\n      };\n      delete params.dateRange;\n      getTemplates(params).then(res => {\n        this.loading = false;\n        if (res.code === 1000) {\n          this.tableData = res.data.records || [];\n          this.total = res.data.total || 0;\n        } else {\n          this.$notify.error({\n            title: 'ÈîôËØØ',\n            message: res.message || 'Ëé∑ÂèñÊï∞ÊçÆÂ§±Ë¥•'\n          });\n        }\n      }).catch(error => {\n        this.loading = false;\n        this.$notify.error({\n          title: 'ÈîôËØØ',\n          message: error.message || 'Ëé∑ÂèñÊï∞ÊçÆÂ§±Ë¥•'\n        });\n      });\n    },\n    // ÈáçÁΩÆÊêúÁ¥¢\n    refresh() {\n      this.search.name = \"\";\n      this.search.dateRange = [];\n      this.query();\n    },\n    // ÂàÜÈ°µÂèòÂåñ\n    handleCurrentChange(val) {\n      this.search.pageNumber = val;\n      this.query();\n    },\n    // ÊØèÈ°µÊù°Êï∞ÂèòÂåñ\n    handleSizeChange(val) {\n      this.search.pageSize = val;\n      this.query();\n    },\n    // ÈÄâÊã©ÂèòÂåñ\n    handleSelectionChange(val) {\n      this.selected = val.map(item => item.id);\n    },\n    // ÊâπÈáèÂà†Èô§\n    deleteDataBtn() {\n      if (this.selected.length === 0) {\n        this.$message.warning('ËØ∑Ëá≥Â∞ëÈÄâÊã©‰∏ÄÊù°Êï∞ÊçÆ');\n        return;\n      }\n      this.$confirm(`Á°ÆÂÆöÂà†Èô§ÈÄâ‰∏≠ÁöÑ ${this.selected.length} Êù°Êï∞ÊçÆ?`, 'ÊèêÁ§∫', {\n        confirmButtonText: 'Á°ÆÂÆö',\n        cancelButtonText: 'ÂèñÊ∂à',\n        type: 'warning',\n        customClass: 'tech-message-box',\n        beforeClose: (action, instance, done) => {\n          if (action === 'confirm') {\n            instance.confirmButtonLoading = true;\n            this.deleteData(this.selected.join(\",\")).finally(() => {\n              instance.confirmButtonLoading = false;\n              done();\n            });\n          } else {\n            done();\n          }\n        }\n      }).catch(() => {\n        this.$message.info('Â∑≤ÂèñÊ∂àÂà†Èô§');\n      });\n    },\n    // Âà†Èô§Êï∞ÊçÆ\n    deleteData(ids) {\n      return deleteTemplate(ids).then(res => {\n        if (res.code === 1000) {\n          this.$message({\n            type: 'success',\n            message: 'Âà†Èô§ÊàêÂäü!',\n            customClass: 'tech-message',\n            duration: 2000\n          });\n          this.search.pageNumber = 1;\n          this.query();\n        } else {\n          this.$notify.error({\n            title: 'ÈîôËØØ',\n            message: res.message || 'Âà†Èô§Â§±Ë¥•'\n          });\n        }\n      }).catch(error => {\n        this.$notify.error({\n          title: 'ÈîôËØØ',\n          message: error.message || 'Âà†Èô§Â§±Ë¥•'\n        });\n      });\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["uploadTemplate","getTemplates","deleteTemplate","getTemplateContent","renderAsync","data","loading","selected","search","name","dateRange","pageNumber","pageSize","total","tableData","methods","tableHeaderStyle","tableRowStyle","beforeUpload","file","isLt10M","size","$message","error","allowedTypes","includes","type","uploadFile","onProgress","onSuccess","onError","formData","FormData","append","onUploadProgress","progressEvent","percentCompleted","Math","round","loaded","percent","then","res","code","$notify","success","title","message","query","Error","catch","viewContent","id","template","find","item","fileType","toLowerCase","fileContent","mimeType","getMimeType","blob","base64ToBlob","fileURL","URL","createObjectURL","window","open","imgWindow","document","write","showDocx","$alert","base64Data","byteCharacters","atob","byteArrays","offset","length","slice","byteNumbers","Array","i","charCodeAt","push","Uint8Array","Blob","map","pdf","jpg","jpeg","png","txt","doc","docx","ppt","pptx","xls","xlsx","fileContentBase64","container","createElement","style","win","body","appendChild","downloadFile","base64","url","link","href","download","click","removeChild","revokeObjectURL","duration","searchPage","params","startDate","endDate","records","refresh","handleCurrentChange","val","handleSizeChange","handleSelectionChange","deleteDataBtn","warning","$confirm","confirmButtonText","cancelButtonText","customClass","beforeClose","action","instance","done","confirmButtonLoading","deleteData","join","finally","info","ids","mounted"],"sources":["src/views/system/template/template.vue"],"sourcesContent":["<template>\r\n  <!-- ÂÆåÊï¥Ê®°ÊùøÈÉ®ÂàÜ‰øùÊåÅ‰∏çÂèò -->\r\n  <div class=\"template-management\">\r\n    <!-- ÊêúÁ¥¢Âå∫Âüü -->\r\n    <div class=\"search-table\">\r\n      <!-- ÊêúÁ¥¢Ë°®Âçï -->\r\n      <div class=\"search\">\r\n        <el-row :gutter=\"15\" style=\"padding:15px\">\r\n          <!-- ËµÑÊñôÂêçÁß∞ÊêúÁ¥¢ -->\r\n          <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n            <div class=\"search-item\">\r\n              <span class=\"search-title\">\r\n                <i class=\"el-icon-document\"></i> ËµÑÊñôÂêçÁß∞:\r\n              </span>\r\n              <el-input\r\n                size=\"small\"\r\n                placeholder=\"ËØ∑ËæìÂÖ•ËµÑÊñôÂêçÁß∞\"\r\n                v-model=\"search.name\"\r\n                class=\"tech-input\">\r\n                <i slot=\"prefix\" class=\"el-icon-edit-outline\"></i>\r\n              </el-input>\r\n            </div>\r\n          </el-col>\r\n          \r\n          <!-- ÂàõÂª∫Êó∂Èó¥ÊêúÁ¥¢ -->\r\n          <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n            <div class=\"search-item\">\r\n              <span class=\"search-title\">\r\n                <i class=\"el-icon-date\"></i> ÂàõÂª∫Êó∂Èó¥:\r\n              </span>\r\n              <el-date-picker\r\n                size=\"small\"\r\n                v-model=\"search.dateRange\"\r\n                type=\"daterange\"\r\n                range-separator=\"Ëá≥\"\r\n                start-placeholder=\"ÂºÄÂßãÊó•Êúü\"\r\n                end-placeholder=\"ÁªìÊùüÊó•Êúü\"\r\n                value-format=\"yyyy-MM-dd\"\r\n                class=\"tech-date-picker\">\r\n              </el-date-picker>\r\n            </div>\r\n          </el-col>\r\n          \r\n          <!-- Êìç‰ΩúÊåâÈíÆ -->\r\n          <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n            <div class=\"search-actions\">\r\n              <el-button \r\n                size=\"small\" \r\n                icon=\"el-icon-search\" \r\n                type=\"primary\" \r\n                @click=\"searchPage\"\r\n                class=\"search-btn\">\r\n                Êü•ËØ¢\r\n              </el-button>\r\n              <el-button \r\n                size=\"small\" \r\n                icon=\"el-icon-refresh\" \r\n                @click=\"refresh\"\r\n                class=\"reset-btn\">\r\n                ÈáçÁΩÆ\r\n              </el-button>\r\n            </div>\r\n          </el-col>\r\n        </el-row>\r\n      </div>\r\n      \r\n      <!-- Ë°®Ê†ºÂå∫Âüü -->\r\n      <div class=\"table\">\r\n        <!-- Êìç‰ΩúÊ†è -->\r\n        <div class=\"action-bar\">\r\n        <el-upload\r\n          class=\"upload-btn\"\r\n          action=\"\"\r\n          :show-file-list=\"false\"\r\n          :before-upload=\"beforeUpload\"\r\n          :http-request=\"uploadFile\">\r\n          <el-button \r\n            type=\"primary\" \r\n            size=\"small\" \r\n            icon=\"el-icon-upload\"\r\n            class=\"action-btn upload-btn\">\r\n            ‰∏ä‰º†ËµÑÊñô\r\n          </el-button>\r\n        </el-upload>\r\n          <el-button \r\n            type=\"danger\" \r\n            size=\"small\" \r\n            icon=\"el-icon-delete\" \r\n            :disabled=\"selected.length <= 0\"\r\n            @click=\"deleteDataBtn\"\r\n            class=\"action-btn delete-btn\">\r\n            Âà†Èô§\r\n          </el-button>\r\n        </div>\r\n        \r\n        <!-- Êï∞ÊçÆË°®Ê†º -->\r\n        <el-table\r\n          v-loading=\"loading\"\r\n          :data=\"tableData\"\r\n          :header-cell-style=\"tableHeaderStyle\"\r\n          :row-style=\"tableRowStyle\"\r\n          @selection-change=\"handleSelectionChange\"\r\n          stripe\r\n          class=\"tech-table\">\r\n          <!-- ÈÄâÊã©Âàó -->\r\n          <el-table-column\r\n            type=\"selection\"\r\n            width=\"55\"\r\n            align=\"center\">\r\n          </el-table-column>\r\n          \r\n          <!-- ËµÑÊñôÂêçÁß∞Âàó -->\r\n          <el-table-column\r\n            prop=\"name\"\r\n            label=\"ËµÑÊñôÂêçÁß∞\"\r\n            width=\"200\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"name-cell\">\r\n                <i class=\"el-icon-document\" style=\"color:#7B68EE; margin-right:8px\"></i>\r\n                <span>{{ row.name }}</span>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          \r\n          <!-- Êñá‰ª∂Á±ªÂûãÂàó -->\r\n          <el-table-column\r\n            prop=\"fileType\"\r\n            label=\"Êñá‰ª∂Á±ªÂûã\"\r\n            width=\"120\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"type-cell\">\r\n                <i class=\"el-icon-files\" style=\"color:#9370DB; margin-right:8px\"></i>\r\n                <span>{{ row.fileType }}</span>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          \r\n          <!-- ÂàõÂª∫Êó∂Èó¥Âàó -->\r\n          <el-table-column\r\n            prop=\"createdAt\"\r\n            label=\"ÂàõÂª∫Êó∂Èó¥\"\r\n            width=\"180\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"time-cell\">\r\n                <i class=\"el-icon-time\" style=\"color:#A5A4BF; margin-right:8px\"></i>\r\n                <span>{{ row.createdAt }}</span>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          \r\n          <!-- ÂàÜ‰∫´ÊùÉÈôêÂàó -->\r\n          <el-table-column\r\n            prop=\"sharePermission\"\r\n            label=\"ÂàÜ‰∫´ÊùÉÈôê\"\r\n            width=\"120\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"permission-cell\">\r\n                <el-tag v-if=\"row.sharePermission === 'VIEW'\" type=\"success\" effect=\"dark\">\r\n                  <i class=\"el-icon-view\"></i> ÂèØÊü•Áúã\r\n                </el-tag>\r\n                <el-tag v-if=\"row.sharePermission === 'EDIT'\" type=\"warning\" effect=\"dark\">\r\n                  <i class=\"el-icon-edit\"></i> ÂèØÁºñËæë\r\n                </el-tag>\r\n                <el-tag v-if=\"row.sharePermission === 'NONE'\" type=\"danger\" effect=\"dark\">\r\n                  <i class=\"el-icon-close\"></i> ‰∏çÂèØËßÅ\r\n                </el-tag>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n          \r\n          <!-- Êìç‰ΩúÂàó -->\r\n          <el-table-column\r\n            label=\"Êìç‰Ωú\"\r\n            width=\"300\">\r\n            <template #default=\"{row}\">\r\n              <div class=\"action-buttons\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  type=\"primary\" \r\n                  @click=\"viewContent(row.id)\"\r\n                  class=\"action-btn view-btn\">\r\n                  <i class=\"el-icon-view\"></i> Êü•Áúã\r\n                </el-button>\r\n                \r\n                <el-button \r\n                  size=\"mini\" \r\n                  type=\"success\" \r\n                  @click=\"downloadFile(row.id)\"\r\n                  class=\"action-btn download-btn\">\r\n                  <i class=\"el-icon-download\"></i> ‰∏ãËΩΩ\r\n                </el-button>\r\n                \r\n                <el-popconfirm\r\n                  title=\"Á°ÆËÆ§Âà†Èô§Ê≠§ËµÑÊñôÔºü\"\r\n                  @confirm=\"deleteData(row.id)\"\r\n                  confirm-button-text=\"Á°ÆËÆ§\"\r\n                  cancel-button-text=\"ÂèñÊ∂à\"\r\n                  icon=\"el-icon-warning\"\r\n                  icon-color=\"#7B68EE\">\r\n                  <el-button \r\n                    size=\"mini\"\r\n                    type=\"danger\"\r\n                    slot=\"reference\"\r\n                    class=\"action-btn delete-btn\">\r\n                    <i class=\"el-icon-delete\"></i> Âà†Èô§\r\n                  </el-button>\r\n                </el-popconfirm>\r\n              </div>\r\n            </template>\r\n          </el-table-column>\r\n        </el-table>\r\n        \r\n        <!-- ÂàÜÈ°µ -->\r\n        <el-pagination\r\n          background\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :page-sizes=\"[10, 20, 50, 100]\"\r\n          :page-size=\"search.pageSize\"\r\n          :current-page=\"search.pageNumber\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\"\r\n          class=\"tech-pagination\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { uploadTemplate, getTemplates, deleteTemplate, getTemplateContent } from '../../../api/api'\r\nimport { renderAsync } from 'docx-preview'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      selected: [],\r\n      search: {\r\n        name: \"\",\r\n        dateRange: [],\r\n        pageNumber: 1,\r\n        pageSize: 10\r\n      },\r\n      total: 0,\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    // Ë°®Ê†ºÂ§¥ÈÉ®Ê†∑Âºè\r\n    tableHeaderStyle() {\r\n      return {\r\n        'color': '#4A2B90',\r\n        'background-color': '#ECE9F4',\r\n        'font-weight': 'bold',\r\n        'border-bottom': '1px solid #7B68EE'\r\n      }\r\n    },\r\n    \r\n    // Ë°®Ê†ºË°åÊ†∑Âºè\r\n    tableRowStyle() {\r\n      return {\r\n        'color': '#5F4B8B',\r\n        'font-size': '14px',\r\n        'border-bottom': '1px solid #F0EEF7'\r\n      }\r\n    },\r\n    \r\n    // ‰∏ä‰º†ÂâçÊ†°È™å\r\n    beforeUpload(file) {\r\n      const isLt10M = file.size / 1024 / 1024 < 10\r\n      if (!isLt10M) {\r\n        this.$message.error('‰∏ä‰º†Êñá‰ª∂Â§ßÂ∞è‰∏çËÉΩË∂ÖËøá10MB!')\r\n        return false\r\n      }\r\n      \r\n      // Ê£ÄÊü•Êñá‰ª∂Á±ªÂûã\r\n      const allowedTypes = [\r\n        'application/pdf',\r\n        'application/msword',\r\n        'application/vnd.openxmlformats-officedocument.wordprocessingml.document',\r\n        'application/vnd.ms-excel',\r\n        'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet',\r\n        'application/vnd.ms-powerpoint',\r\n        'application/vnd.openxmlformats-officedocument.presentationml.presentation',\r\n        'image/jpeg',\r\n        'image/png'\r\n      ]\r\n      \r\n      if (!allowedTypes.includes(file.type)) {\r\n        this.$message.error('Âè™ÊîØÊåÅ‰∏ä‰º†PDF„ÄÅWord„ÄÅExcel„ÄÅPPT„ÄÅJPGÂíåPNGÊñá‰ª∂!')\r\n        return false\r\n      }\r\n      \r\n      return true\r\n    },\r\n    \r\n    // Êñá‰ª∂‰∏ä‰º†ÊñπÊ≥ïÔºàÂÆåÊï¥ÂÆûÁé∞Ôºâ\r\n    uploadFile({ file, onProgress, onSuccess, onError }) {\r\n      const formData = new FormData();\r\n      formData.append('file', file); // ‚úÖ ÂÖ≥ÈîÆÁÇπÔºöÂøÖÈ°ªÊòØ file\r\n\r\n      uploadTemplate(formData, {\r\n        // headers: { 'Content-Type': 'multipart/form-data' },\r\n        onUploadProgress: (progressEvent) => {\r\n          const percentCompleted = Math.round((progressEvent.loaded * 100) / progressEvent.total);\r\n          onProgress({ percent: percentCompleted }); // ÊîØÊåÅËøõÂ∫¶Êù°\r\n        }\r\n      }).then(res => {\r\n        if (res.code === 1000) {\r\n          this.$notify.success({ title: 'ÊàêÂäü', message: '‰∏ä‰º†ÊàêÂäü' });\r\n          this.query();\r\n          onSuccess(res); // ‚úÖ ÂøÖÈ°ªÂõûË∞ÉÊàêÂäü\r\n        } else {\r\n          this.$notify.error({ title: 'ÈîôËØØ', message: res.message || '‰∏ä‰º†Â§±Ë¥•' });\r\n          onError(new Error(res.message || '‰∏ä‰º†Â§±Ë¥•')); // ‚úÖ ÂõûË∞ÉÂ§±Ë¥•\r\n        }\r\n      }).catch(error => {\r\n        this.$notify.error({ title: 'ÈîôËØØ', message: error.message || '‰∏ä‰º†Â§±Ë¥•' });\r\n        onError(error); // ‚úÖ ÂõûË∞ÉÂ§±Ë¥•\r\n      });\r\n    },\r\n    \r\n    // Êü•ÁúãÂÜÖÂÆπ Âú®Á∫øÈ¢ÑËßàdocx ÊàëÊé•‰∫Ü‰∏Ä‰∏™Ë∞∑Ê≠åÁöÑÈìæÊé•\r\n    viewContent(id) {\r\n        this.loading = true;\r\n        getTemplateContent({ id }).then(res => {\r\n          this.loading = false;\r\n          if (res.code === 1000) {\r\n            const template = this.tableData.find(item => item.id === id);\r\n            const data = res.data;\r\n            const fileType = template.fileType?.toLowerCase();\r\n\r\n            if (!data.fileContent) {\r\n              this.$message.error('Êú™Ëé∑ÂèñÂà∞Êñá‰ª∂ÂÜÖÂÆπ');\r\n              return;\r\n            }\r\n\r\n            const mimeType = this.getMimeType(fileType);\r\n            const blob = this.base64ToBlob(data.fileContent, mimeType);\r\n            const fileURL = URL.createObjectURL(blob);\r\n\r\n            if (fileType === 'pdf') {\r\n              window.open(fileURL, '_blank');\r\n            } else if (['jpg', 'jpeg', 'png'].includes(fileType)) {\r\n              const imgWindow = window.open('', '_blank');\r\n              imgWindow.document.write(`<img src=\"${fileURL}\" style=\"max-width:100%\"/>`);\r\n            } else if (fileType === 'docx') {\r\n              this.showDocx(data.fileContent); // üëà ‰ΩøÁî® docx-preview Ê∏≤Êüì\r\n            } else {\r\n              this.$alert('ËØ•Ê†ºÂºèÊöÇ‰∏çÊîØÊåÅÂú®Á∫øÈ¢ÑËßàÔºåËØ∑‰∏ãËΩΩÊü•Áúã', 'ÊèêÁ§∫');\r\n            }\r\n          } else {\r\n            this.$notify.error({\r\n              title: 'ÈîôËØØ',\r\n              message: res.message || 'Ëé∑ÂèñÂÜÖÂÆπÂ§±Ë¥•'\r\n            });\r\n          }\r\n        }).catch(error => {\r\n          this.loading = false;\r\n          this.$notify.error({\r\n            title: 'ÈîôËØØ',\r\n            message: error.message || 'Ëé∑ÂèñÂÜÖÂÆπÂ§±Ë¥•'\r\n          });\r\n        });\r\n      },\r\n\r\n\r\n\r\n    //Êñá‰ª∂‰∫åËøõÂà∂ËΩ¨Âåñ\r\n    base64ToBlob(base64Data, mimeType = '') {\r\n      const byteCharacters = atob(base64Data);\r\n      const byteArrays = [];\r\n\r\n      for (let offset = 0; offset < byteCharacters.length; offset += 512) {\r\n        const slice = byteCharacters.slice(offset, offset + 512);\r\n        const byteNumbers = new Array(slice.length);\r\n        for (let i = 0; i < slice.length; i++) {\r\n          byteNumbers[i] = slice.charCodeAt(i);\r\n        }\r\n        byteArrays.push(new Uint8Array(byteNumbers));\r\n      }\r\n\r\n      return new Blob(byteArrays, { type: mimeType });\r\n    },\r\n\r\n    getMimeType(fileType) {\r\n      const map = {\r\n        pdf: 'application/pdf',\r\n        jpg: 'image/jpeg',\r\n        jpeg: 'image/jpeg',\r\n        png: 'image/png',\r\n        txt: 'text/plain',\r\n        doc: 'application/msword',\r\n        docx: 'application/vnd.openxmlformats-officedocument.wordprocessingml.document',\r\n        ppt: 'application/vnd.ms-powerpoint',\r\n        pptx: 'application/vnd.openxmlformats-officedocument.presentationml.presentation',\r\n        xls: 'application/vnd.ms-excel',\r\n        xlsx: 'application/vnd.openxmlformats-officedocument.spreadsheetml.sheet',\r\n      };\r\n      return map[fileType] || 'application/octet-stream';\r\n    },\r\n\r\n\r\n  //pptx excel docxÈ¢ÑËßà\r\n  showDocx(fileContentBase64) {\r\n    const blob = this.base64ToBlob(fileContentBase64, 'application/vnd.openxmlformats-officedocument.wordprocessingml.document');\r\n    const container = document.createElement('div');\r\n    container.style = 'padding:20px;max-height:90vh;overflow:auto;';\r\n    renderAsync(blob, container).then(() => {\r\n      const win = window.open('', '_blank');\r\n      win.document.body.appendChild(container);\r\n    });\r\n  },\r\n\r\n\r\n    \r\n    // ‰∏ãËΩΩÊñá‰ª∂\r\n    downloadFile(id) {\r\n      this.loading = true;\r\n      getTemplateContent({ id }).then(res => {\r\n        this.loading = false;\r\n        if (res.code === 1000) {\r\n          const template = this.tableData.find(item => item.id === id);\r\n          const base64 = res.data.fileContent;\r\n          const mimeType = this.getMimeType(template.fileType);\r\n          const blob = this.base64ToBlob(base64, mimeType);\r\n          const url = URL.createObjectURL(blob);\r\n          \r\n          const link = document.createElement('a');\r\n          link.href = url;\r\n          link.download = template.name || 'download';\r\n          document.body.appendChild(link);\r\n          link.click();\r\n          document.body.removeChild(link);\r\n          URL.revokeObjectURL(url);\r\n          \r\n          this.$notify.success({\r\n            title: 'ÊàêÂäü',\r\n            message: 'Êñá‰ª∂‰∏ãËΩΩÂ∑≤ÂºÄÂßã',\r\n            duration: 2000\r\n          });\r\n        } else {\r\n          this.$notify.error({\r\n            title: 'ÈîôËØØ',\r\n            message: res.message || '‰∏ãËΩΩÂ§±Ë¥•'\r\n          });\r\n        }\r\n      }).catch(error => {\r\n        this.loading = false;\r\n        this.$notify.error({\r\n          title: 'ÈîôËØØ',\r\n          message: error.message || '‰∏ãËΩΩÂ§±Ë¥•'\r\n        });\r\n      });\r\n    },\r\n\r\n    \r\n    // ÊêúÁ¥¢\r\n    searchPage() {\r\n      this.search.pageNumber = 1\r\n      this.query()\r\n    },\r\n    \r\n    // Êü•ËØ¢Êï∞ÊçÆ\r\n    query() {\r\n      this.loading = true\r\n      const params = {\r\n        ...this.search,\r\n        startDate: this.search.dateRange ? this.search.dateRange[0] : '',\r\n        endDate: this.search.dateRange ? this.search.dateRange[1] : ''\r\n      }\r\n      delete params.dateRange\r\n      \r\n      getTemplates(params).then(res => {\r\n        this.loading = false\r\n        if (res.code === 1000) {\r\n          this.tableData = res.data.records || []\r\n          this.total = res.data.total || 0\r\n        } else {\r\n          this.$notify.error({\r\n            title: 'ÈîôËØØ',\r\n            message: res.message || 'Ëé∑ÂèñÊï∞ÊçÆÂ§±Ë¥•'\r\n          })\r\n        }\r\n      }).catch(error => {\r\n        this.loading = false\r\n        this.$notify.error({\r\n          title: 'ÈîôËØØ',\r\n          message: error.message || 'Ëé∑ÂèñÊï∞ÊçÆÂ§±Ë¥•'\r\n        })\r\n      })\r\n    },\r\n    \r\n    // ÈáçÁΩÆÊêúÁ¥¢\r\n    refresh() {\r\n      this.search.name = \"\"\r\n      this.search.dateRange = []\r\n      this.query()\r\n    },\r\n    \r\n    // ÂàÜÈ°µÂèòÂåñ\r\n    handleCurrentChange(val) {\r\n      this.search.pageNumber = val\r\n      this.query()\r\n    },\r\n    \r\n    // ÊØèÈ°µÊù°Êï∞ÂèòÂåñ\r\n    handleSizeChange(val) {\r\n      this.search.pageSize = val\r\n      this.query()\r\n    },\r\n    \r\n    // ÈÄâÊã©ÂèòÂåñ\r\n    handleSelectionChange(val) {\r\n      this.selected = val.map(item => item.id)\r\n    },\r\n    \r\n    // ÊâπÈáèÂà†Èô§\r\n    deleteDataBtn() {\r\n      if (this.selected.length === 0) {\r\n        this.$message.warning('ËØ∑Ëá≥Â∞ëÈÄâÊã©‰∏ÄÊù°Êï∞ÊçÆ')\r\n        return\r\n      }\r\n      \r\n      this.$confirm(`Á°ÆÂÆöÂà†Èô§ÈÄâ‰∏≠ÁöÑ ${this.selected.length} Êù°Êï∞ÊçÆ?`, 'ÊèêÁ§∫', {\r\n        confirmButtonText: 'Á°ÆÂÆö',\r\n        cancelButtonText: 'ÂèñÊ∂à',\r\n        type: 'warning',\r\n        customClass: 'tech-message-box',\r\n        beforeClose: (action, instance, done) => {\r\n          if (action === 'confirm') {\r\n            instance.confirmButtonLoading = true\r\n            this.deleteData(this.selected.join(\",\"))\r\n              .finally(() => {\r\n                instance.confirmButtonLoading = false\r\n                done()\r\n              })\r\n          } else {\r\n            done()\r\n          }\r\n        }\r\n      }).catch(() => {\r\n        this.$message.info('Â∑≤ÂèñÊ∂àÂà†Èô§')\r\n      })\r\n    },\r\n    \r\n    // Âà†Èô§Êï∞ÊçÆ\r\n    deleteData(ids) {\r\n      return deleteTemplate(ids).then(res => {\r\n        if (res.code === 1000) {\r\n          this.$message({\r\n            type: 'success',\r\n            message: 'Âà†Èô§ÊàêÂäü!',\r\n            customClass: 'tech-message',\r\n            duration: 2000\r\n          })\r\n          this.search.pageNumber = 1\r\n          this.query()\r\n        } else {\r\n          this.$notify.error({\r\n            title: 'ÈîôËØØ',\r\n            message: res.message || 'Âà†Èô§Â§±Ë¥•'\r\n          })\r\n        }\r\n      }).catch(error => {\r\n        this.$notify.error({\r\n          title: 'ÈîôËØØ',\r\n          message: error.message || 'Âà†Èô§Â§±Ë¥•'\r\n        })\r\n      })\r\n    },\r\n  },\r\n\r\n  \r\n  mounted() {\r\n    this.query()\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.template-management {\r\n  padding: 20px;\r\n  background-color: #F8F7FC;\r\n}\r\n\r\n.search-table {\r\n  width: 100%;\r\n}\r\n\r\n.search {\r\n  background: #ffffff;\r\n  border-radius: 8px;\r\n  padding: 15px;\r\n  margin-bottom: 20px;\r\n  box-shadow: 0 2px 12px rgba(123, 104, 238, 0.1);\r\n}\r\n\r\n.search-item {\r\n  display: flex;\r\n  flex-direction: column;\r\n}\r\n\r\n.search-title {\r\n  font-size: 14px;\r\n  color: #4A2B90;\r\n  margin-bottom: 8px;\r\n  display: flex;\r\n  align-items: center;\r\n}\r\n\r\n.search-title i {\r\n  marg