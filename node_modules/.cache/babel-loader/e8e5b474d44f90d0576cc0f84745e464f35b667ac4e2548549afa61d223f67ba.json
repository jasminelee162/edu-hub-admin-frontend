{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { mapState } from 'vuex';\nexport default {\n  name: \"asside\",\n  data() {\n    return {\n      isCollapse: false,\n      textColor: \"#A4B7CC\"\n    };\n  },\n  computed: {\n    ...mapState({\n      activeMenuArrary: state => state.menu.activeMenuArrary,\n      displayMenus: state => state.menu.displayMenus\n    })\n  },\n  methods: {\n    handleSelect(key) {\n      var menu = key.split(\"@\");\n      this.$router.push({\n        path: menu[0]\n      });\n      this.$store.commit('menu/setActiveMenu', menu[0]);\n      for (let i = 0; i < this.activeMenuArrary.length; i++) {\n        if (this.activeMenuArrary[i].url == menu[0]) {\n          return;\n        }\n      }\n      var param = {\n        \"name\": menu[1],\n        \"url\": menu[0]\n      };\n      this.$store.commit('menu/addActiveMenu', param);\n    }\n  },\n  created() {},\n  mounted() {\n    // 监听collapse\n    this.$bus.$on('collapse', res => {\n      this.isCollapse = res;\n    });\n    this.$store.commit('menu/setActiveMenu', this.$route.path);\n    for (let i = 0; i < this.activeMenuArrary.length; i++) {\n      if (this.activeMenuArrary[i].url == this.$route.path) {\n        return;\n      }\n    }\n    var param = {\n      \"name\": this.$route.name,\n      \"url\": this.$route.path\n    };\n    this.$store.commit('menu/addActiveMenu', param);\n  }\n};","map":{"version":3,"names":["mapState","name","data","isCollapse","textColor","computed","activeMenuArrary","state","menu","displayMenus","methods","handleSelect","key","split","$router","push","path","$store","commit","i","length","url","param","created","mounted","$bus","$on","res","$route"],"sources":["src/components/layout/aside.vue"],"sourcesContent":["<template>\r\n  <el-aside :width=\"isCollapse ? '72px' : '240px'\" style=\"background-color: #1A1C2A; border-right: 1px solid #2C2E3A\">\r\n  <div class=\"back-icon\" :style=\"{\r\n    width: isCollapse ? '71px' : '239px',\r\n    background: '#1A1C2A',\r\n    borderBottom: '1px solid rgba(255,255,255,0.05)'\r\n  }\">\r\n    <span class=\"logo-text\" :class=\"isCollapse? 'xiaoshi':''\">\r\n      <span style=\"background: rgba(124, 77, 255, 0.15); color:#E1E1FF; padding: 4px 8px; border-radius: 4px\">\r\n        <i class=\"el-icon-monitor\" style=\"color: #7E57C2\"></i>\r\n        <span v-if=\"!isCollapse\"> 教育平台</span>\r\n      </span>\r\n    </span>\r\n  </div>\r\n        <el-menu\r\n            @select=\"handleSelect\"\r\n            :collapse=\"isCollapse\"\r\n            :collapse-transition=\"false\"\r\n            class=\"el-menu-vertical\"\r\n            :unique-opened=\"true\"\r\n            background-color=\"#222B40\"\r\n            :text-color=\"textColor\"\r\n            :default-active=\"this.$route.path + '@' + this.$route.name\"\r\n            active-text-color=\"#ffffff\">\r\n            <span v-for=\"(item,index) in displayMenus\" :key=\"index\">\r\n                <el-menu-item v-if=\"item.menuType == 1\" :index=\"item.routeUrl + '@' + item.menuName\">\r\n                    <i :class=\"item.icon\"></i>\r\n                    <span slot=\"title\">{{item.menuName}}</span>\r\n                </el-menu-item>\r\n                <el-submenu v-if=\"item.menuType == 0\" :index=\"index+''\">\r\n                    <template slot=\"title\">\r\n                        <i :class=\"item.icon\"></i>\r\n                        <span v-if=\"!isCollapse\">{{item.menuName}}</span>\r\n                    </template>\r\n                    <span v-for=\"(element,subscript) in item.child\" :key=\"subscript\">\r\n                        <el-submenu v-if=\"element.menuType == 0\" :index=\"index+'-'+subscript\">\r\n                            <template slot=\"title\">\r\n                                <i :class=\"element.icon\"></i>\r\n                                <span>{{element.menuName}}</span>\r\n                            </template>\r\n                            <el-menu-item v-for=\"(one,sub) in element.child\" v-if=\"one.menuType == 1\" :index=\"one.routeUrl + '@' + one.menuName\">\r\n                                <i :class=\"one.icon\"></i>\r\n                                <span slot=\"title\">{{one.menuName}}</span>\r\n                            </el-menu-item>\r\n                        </el-submenu>\r\n                        <el-menu-item v-if=\"element.menuType == 1\" :index=\"element.routeUrl + '@' + element.menuName\">\r\n                            <i :class=\"element.icon\"></i>\r\n                            <span slot=\"title\">{{element.menuName}}</span>\r\n                        </el-menu-item>\r\n                    </span>\r\n                </el-submenu>\r\n            </span>\r\n        </el-menu>\r\n    </el-aside>\r\n</template>\r\n\r\n<script>\r\n  import { mapState } from 'vuex'\r\n  export default {\r\n    name: \"asside\",\r\n    data() {\r\n      return{\r\n        isCollapse: false,\r\n        textColor: \"#A4B7CC\"\r\n      }\r\n    },\r\n    computed: {\r\n      ...mapState({\r\n          activeMenuArrary: state => state.menu.activeMenuArrary,\r\n          displayMenus: state => state.menu.displayMenus,\r\n      })\r\n    },\r\n    methods: {\r\n        handleSelect(key) {\r\n            var menu = key.split(\"@\")\r\n            this.$router.push({\r\n                path: menu[0],\r\n            })\r\n            this.$store.commit('menu/setActiveMenu', menu[0])\r\n            for(let i = 0;i < this.activeMenuArrary.length;i++) {\r\n                if(this.activeMenuArrary[i].url == menu[0]) {\r\n                    return;\r\n                }\r\n            }\r\n            var param = {\r\n                \"name\": menu[1],\r\n                \"url\": menu[0]\r\n            }\r\n            this.$store.commit('menu/addActiveMenu', param)\r\n        }\r\n    },\r\n    created() {\r\n\r\n    },\r\n    mounted() {\r\n        // 监听collapse\r\n        this.$bus.$on('collapse', res=>{\r\n            this.isCollapse = res\r\n        })\r\n        this.$store.commit('menu/setActiveMenu', this.$route.path)\r\n        for(let i = 0;i < this.activeMenuArrary.length;i++) {\r\n            if(this.activeMenuArrary[i].url == this.$route.path) {\r\n                return;\r\n            }\r\n        }\r\n        var param = {\r\n            \"name\": this.$route.name,\r\n            \"url\": this.$route.path\r\n        }\r\n        this.$store.commit('menu/addActiveMenu', param)\r\n    },\r\n }\r\n</script>\r\n\r\n<style scoped lang=\"scss\">\r\n    .el-aside {\r\n        background: #1A1C2A !important;\r\n        transition: all 0.4s cubic-bezier(0.4, 0, 0.2, 1);\r\n    }\r\n    .back-icon {\r\n        display: flex;\r\n        height: 100px;\r\n        background-color: #222B40;\r\n        justify-content: center;\r\n        align-items: center;\r\n        border-bottom: 1px solid rgba(255, 255, 255, 0.1);\r\n    }\r\n    .logo {\r\n        widows: 50px;\r\n        height: 50px;\r\n    }\r\n    .logo-text {\r\n        padding-left: 6px;\r\n        /* Bold/19px */\r\n        color: #fff;\r\n        font-family: Mulish;\r\n        font-size: 15px;\r\n        font-weight: 700;\r\n        line-height: 24;\r\n        letter-spacing: 0.4000000059604645px;\r\n        text-shadow: 0 2px 4px rgba(0, 0, 0, 0.2);\r\n    }\r\n    .el-menu {\r\n    background: transparent !important;\r\n    border-right: none !important;\r\n\r\n    /* 菜单项基础样式 */\r\n    &:deep(.el-menu-item),\r\n    &:deep(.el-submenu__title) {\r\n        color: #E1E1FF;\r\n        height: 48px !important;\r\n        line-height: 48px !important;\r\n        margin: 4px 8px;\r\n        border-radius: 4px;\r\n        \r\n        i {\r\n        color: #7E57C2;\r\n        font-size: 18px;\r\n        }\r\n\r\n        /* 悬停效果 */\r\n        &:hover {\r\n        background: rgba(94, 53, 177, 0.2) !important;\r\n        color: #FFFFFF;\r\n        \r\n        i {\r\n            color: #B388FF;\r\n        }\r\n        }\r\n    }\r\n\r\n    /* 激活状态 */\r\n    &:deep(.el-menu-item.is-active) {\r\n        background: linear-gradient(90deg, rgba(124, 77, 255, 0.15), transparent) !important;\r\n        color: #FFFFFF !important;\r\n        position: relative;\r\n        \r\n        &::after {\r\n        content: '';\r\n        position: absolute;\r\n        left: 0;\r\n        top: 0;\r\n        height: 100%;\r\n        width: 3px;\r\n        background: linear-gradient(to bottom, #7E57C2, #5C6BC0);\r\n        }\r\n    }\r\n  }\r\n\r\n    .el-menu::-webkit-scrollbar,.el-aside::-webkit-scrollbar {\r\n        width: 0px;\r\n        height: 0px;\r\n    }\r\n    .el-submenu>>>.el-submenu__title:hover {\r\n        color:#EAB040!important;\r\n        background-color: #3E3F49!important;\r\n    }\r\n    .el-menu-vertical:deep(.el-menu-item:hover){\r\n        color:#0863F2!important;\r\n        background-color: #001528!important;\r\n    }\r\n    .el-menu-vertical:deep(.el-menu-item.is-active) {\r\n        background-color: #001528!important;\r\n        box-shadow:inset 5px 0 #DDE2FF;\r\n    }\r\n    .el-menu-item, .el-submenu__title {\r\n        height: 47px !important;\r\n        line-height: 47px !important;\r\n    }\r\n    .el-aside .el-menu {\r\n        border-right: none !important;\r\n    }\r\n    .el-submenu .el-menu-item {\r\n        background-color: #1B2233!important;\r\n    }\r\n    .el-submenu .el-submenu:deep(.el-submenu__title) {\r\n        background-color: #1B2233!important;\r\n    }\r\n     .el-submenu:deep(.el-menu) {\r\n    background: rgba(48, 0, 99, 0.3) !important;\r\n    \r\n     /* 子菜单样式 */\r\n  .el-submenu:deep(.el-menu) {\r\n    background: rgba(26, 28, 42, 0.8) !important;\r\n    \r\n    .el-menu-item {\r\n      padding-left: 54px !important;\r\n    }\r\n  }\r\n}\r\n\r\n/* Logo区域微调 */\r\n.back-icon {\r\n  transition: all 0.3s;\r\n  .logo-text {\r\n    font-weight: 500;\r\n    letter-spacing: 0.5px;\r\n    i {\r\n      vertical-align: middle;\r\n    }\r\n  }\r\n}\r\n</style>\r\n