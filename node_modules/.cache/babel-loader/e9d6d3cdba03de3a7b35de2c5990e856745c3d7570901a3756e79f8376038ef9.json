{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getApeArticlePage, removeApeArticle, editApeArticle } from '../../../api/api';\nimport add from '../../../components/system/article/addArticle';\nimport update from '../../../components/system/article/updateArticle';\nexport default {\n  data() {\n    return {\n      loading: true,\n      update: [],\n      remove: [],\n      updateId: \"\",\n      addVisible: false,\n      updateVisible: false,\n      taskId: \"\",\n      type: 1,\n      search: {\n        title: \"\",\n        state: \"\",\n        taskName: \"\",\n        pageNumber: 1,\n        pageSize: 10,\n        type: 1\n      },\n      total: 0,\n      tableData: []\n    };\n  },\n  components: {\n    add,\n    update\n  },\n  methods: {\n    tableHeaderStyle() {\n      return {\n        'color': '#4A2B90',\n        'background-color': '#ECE9F4',\n        'font-weight': '600',\n        'border-bottom': '1px solid #D8D0F0',\n        'font-family': 'Microsoft YaHei'\n      };\n    },\n    tableRowStyle() {\n      return {\n        'color': '#5F4B8B',\n        'font-size': '14px',\n        'font-family': 'Microsoft YaHei',\n        'border-bottom': '1px solid #F0EEF7'\n      };\n    },\n    toArticleComment(id) {\n      var param = {\n        \"name\": \"文章评论\",\n        \"url\": \"/articleComment?id=\" + id\n      };\n      this.$store.commit('menu/addActiveMenu', param);\n      this.$router.push(\"/articleComment?id=\" + id);\n      this.$store.commit('menu/setActiveMenu', \"/articleComment?id=\" + id);\n    },\n    listing(id) {\n      var param = {\n        id: id,\n        state: 0\n      };\n      editApeArticle(param).then(res => {\n        if (res.code == 1000) {\n          this.$notify.success({\n            title: '成功',\n            message: \"审核成功\"\n          });\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    offShelf(id) {\n      var param = {\n        id: id,\n        state: 1\n      };\n      editApeArticle(param).then(res => {\n        if (res.code == 1000) {\n          this.$notify.success({\n            title: '成功',\n            message: \"审核成功\"\n          });\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    searchPage() {\n      this.search.pageNumber = 1;\n      this.query();\n    },\n    query() {\n      getApeArticlePage(this.search).then(res => {\n        if (res.code == 1000) {\n          this.tableData = res.data.records;\n          this.total = res.data.total;\n          this.loading = false;\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    refresh() {\n      this.search.title = \"\";\n      this.search.state = \"\";\n      this.search.taskName = \"\";\n      this.query();\n    },\n    handleCurrentChange(val) {\n      this.search.pageNumber = val;\n      this.query();\n    },\n    handleSizeChange(val) {\n      this.search.pageSize = val;\n      this.query();\n    },\n    handleSelectionChange(val) {\n      this.update = [];\n      this.remove = [];\n      for (let i = 0; i < val.length; i++) {\n        var item = val[i];\n        this.update.push(item.id);\n        this.remove.push(item.id);\n      }\n    },\n    add() {\n      this.addVisible = true;\n    },\n    addFalse() {\n      this.addVisible = false;\n      this.query();\n    },\n    updateFalse() {\n      this.updateId = '';\n      this.updateVisible = false;\n      this.query();\n    },\n    updateData(id) {\n      this.updateId = id;\n      this.updateVisible = true;\n    },\n    updateDataBtn() {\n      this.updateData(this.update[0]);\n    },\n    deleteDataBtn() {\n      this.$confirm('确定删除选中的' + this.remove.length + '条数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        customClass: 'purple-message-box'\n      }).then(() => {\n        this.deleteDate(this.remove.join(\",\"));\n      }).catch(() => {});\n    },\n    deleteDate(ids) {\n      removeApeArticle({\n        ids: ids\n      }).then(res => {\n        if (res.code == 1000) {\n          this.$message({\n            type: 'success',\n            message: '删除成功!',\n            customClass: 'purple-message'\n          });\n          this.pageNumber = 1;\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    var name = this.$route.query.name;\n    var id = this.$route.query.id;\n    var type = this.$route.query.type;\n    if (name) {\n      this.search.taskName = name;\n    }\n    if (id) {\n      this.taskId = id;\n    }\n    if (type) {\n      this.type = type;\n    }\n    this.query();\n  }\n};","map":{"version":3,"names":["getApeArticlePage","removeApeArticle","editApeArticle","add","update","data","loading","remove","updateId","addVisible","updateVisible","taskId","type","search","title","state","taskName","pageNumber","pageSize","total","tableData","components","methods","tableHeaderStyle","tableRowStyle","toArticleComment","id","param","$store","commit","$router","push","listing","then","res","code","$notify","success","message","query","error","offShelf","searchPage","records","refresh","handleCurrentChange","val","handleSizeChange","handleSelectionChange","i","length","item","addFalse","updateFalse","updateData","updateDataBtn","deleteDataBtn","$confirm","confirmButtonText","cancelButtonText","customClass","deleteDate","join","catch","ids","$message","mounted","name","$route"],"sources":["src/views/system/testP/teacherTest.vue"],"sourcesContent":["<template>\r\n  <div class=\"article-container\">\r\n    <div class=\"search-card\">\r\n      <el-row :gutter=\"15\">\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-field\">\r\n            <span class=\"search-label\">\r\n              <i class=\"el-icon-collection-tag\"></i> 课程名称:\r\n            </span>\r\n            <el-input\r\n              :disabled=\"type == 0\"\r\n              size=\"small\"\r\n              placeholder=\"请输入课程名称\"\r\n              v-model=\"search.taskName\"\r\n              class=\"purple-input\">\r\n              <i slot=\"prefix\" class=\"el-icon-notebook-2\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>  \r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-field\">\r\n            <span class=\"search-label\">\r\n              <i class=\"el-icon-edit-outline\"></i> 标题:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入标题\"\r\n              v-model=\"search.title\"\r\n              class=\"purple-input\">\r\n              <i slot=\"prefix\" class=\"el-icon-document\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col> \r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-field\">\r\n            <span class=\"search-label\">\r\n              <i class=\"el-icon-s-flag\"></i> 状态:\r\n            </span>\r\n            <el-select \r\n              clearable \r\n              size=\"small\" \r\n              v-model=\"search.state\" \r\n              placeholder=\"请选择状态\"\r\n              class=\"purple-select\">\r\n              <el-option label=\"审核通过\" value=\"0\"></el-option>\r\n              <el-option label=\"未通过\" value=\"1\"></el-option>\r\n              <el-option label=\"待审核\" value=\"2\"></el-option>\r\n            </el-select>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-actions\">\r\n            <el-button \r\n              size=\"small\" \r\n              icon=\"el-icon-search\" \r\n              type=\"primary\" \r\n              @click=\"searchPage\"\r\n              class=\"search-button purple-gradient\">\r\n              查询\r\n            </el-button>\r\n            <el-button \r\n              size=\"small\" \r\n              icon=\"el-icon-refresh-left\" \r\n              @click=\"refresh\"\r\n              class=\"reset-button\">\r\n              重置\r\n            </el-button>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <div class=\"data-card\">\r\n      <div class=\"toolbar\">\r\n        <el-button \r\n          type=\"primary\" \r\n          size=\"small\" \r\n          icon=\"el-icon-plus\" \r\n          @click=\"add\"\r\n          class=\"tool-button add-button\">\r\n          新增\r\n        </el-button>\r\n        <el-button \r\n          type=\"success\" \r\n          size=\"small\" \r\n          icon=\"el-icon-edit\" \r\n          :disabled=\"update.length !== 1\"\r\n          @click=\"updateDataBtn\"\r\n          class=\"tool-button edit-button\">\r\n          修改\r\n        </el-button>\r\n        <el-button \r\n          type=\"danger\" \r\n          size=\"small\" \r\n          icon=\"el-icon-delete\" \r\n          :disabled=\"update.length <= 0\"\r\n          @click=\"deleteDataBtn\"\r\n          class=\"tool-button delete-button\">\r\n          删除\r\n        </el-button>\r\n      </div>\r\n\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"tableData\"\r\n        :header-cell-style=\"tableHeaderStyle\"\r\n        :row-style=\"tableRowStyle\"\r\n        @selection-change=\"handleSelectionChange\"\r\n        stripe\r\n        class=\"purple-table\">\r\n        <el-table-column type=\"selection\" width=\"55\" align=\"center\"></el-table-column>\r\n        <el-table-column prop=\"taskName\" label=\"课程名称\" width=\"120\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"cell-content\">\r\n              <i class=\"el-icon-collection-tag\" style=\"color:#7B68EE\"></i>\r\n              <span>{{ row.taskName }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"title\" label=\"标题\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"cell-content\">\r\n              <i class=\"el-icon-document\" style=\"color:#9370DB\"></i>\r\n              <span>{{ row.title }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"state\" label=\"状态\" width=\"100\">\r\n          <template #default=\"{row}\">\r\n            <el-tag \r\n              :type=\"row.state == 0 ? 'success' : row.state == 1 ? 'danger' : 'warning'\"\r\n              class=\"status-tag\">\r\n              {{ row.state == 0 ? '通过' : row.state == 1 ? '未通过' : '待审核' }}\r\n            </el-tag>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"createBy\" label=\"创建者\" width=\"100\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"cell-content\">\r\n              <i class=\"el-icon-user\" style=\"color:#A5A4BF\"></i>\r\n              <span>{{ row.createBy }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column prop=\"createTime\" label=\"创建时间\" width=\"150\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"cell-content\">\r\n              <i class=\"el-icon-time\" style=\"color:#A5A4BF\"></i>\r\n              <span>{{ row.createTime }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column label=\"操作\" width=\"320\" fixed=\"right\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"action-buttons\">\r\n              <el-tooltip content=\"审核通过\" placement=\"top\" v-if=\"row.state != 0\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  @click=\"listing(row.id)\"\r\n                  class=\"action-button approve-button\">\r\n                  <i class=\"el-icon-check\"></i>\r\n                </el-button>\r\n              </el-tooltip>\r\n              \r\n              <el-tooltip content=\"审核不通过\" placement=\"top\" v-if=\"row.state != 1\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  @click=\"offShelf(row.id)\"\r\n                  class=\"action-button reject-button\">\r\n                  <i class=\"el-icon-close\"></i>\r\n                </el-button>\r\n              </el-tooltip>\r\n              \r\n              <el-tooltip content=\"查看评论\" placement=\"top\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  @click=\"toArticleComment(row.id)\"\r\n                  class=\"action-button comment-button\">\r\n                  <i class=\"el-icon-chat-dot-round\"></i>\r\n                </el-button>\r\n              </el-tooltip>\r\n              \r\n              <el-tooltip content=\"修改文章\" placement=\"top\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  @click=\"updateData(row.id)\"\r\n                  class=\"action-button edit-button\">\r\n                  <i class=\"el-icon-edit\"></i>\r\n                </el-button>\r\n              </el-tooltip>\r\n              \r\n              <el-popconfirm\r\n                title=\"确认删除此文章？\"\r\n                @confirm=\"deleteDate(row.id)\"\r\n                confirm-button-text=\"确认\"\r\n                cancel-button-text=\"取消\"\r\n                icon=\"el-icon-warning\"\r\n                icon-color=\"#7B68EE\">\r\n                <el-tooltip content=\"删除文章\" placement=\"top\">\r\n                  <el-button \r\n                    size=\"mini\"\r\n                    class=\"action-button delete-button\">\r\n                    <i class=\"el-icon-delete\"></i>\r\n                  </el-button>\r\n                </el-tooltip>\r\n              </el-popconfirm>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n\r\n      <el-pagination\r\n        background\r\n        layout=\"total, sizes, prev, pager, next, jumper\"\r\n        :page-sizes=\"[10, 20, 50, 100]\"\r\n        :page-size=\"search.pageSize\"\r\n        :current-page=\"search.pageNumber\"\r\n        @size-change=\"handleSizeChange\"\r\n        @current-change=\"handleCurrentChange\"\r\n        :total=\"total\"\r\n        class=\"purple-pagination\">\r\n      </el-pagination>\r\n    </div>\r\n    \r\n    <add @addFalse=\"addFalse\" flag=\"2\" :taskId=\"taskId\" :type=\"type\" :addVisible=\"addVisible\"></add>\r\n    <update @updateFalse=\"updateFalse\" flag=\"2\" :taskId=\"taskId\" :type=\"type\" :updateId=\"updateId\" :updateVisible=\"updateVisible\"></update>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport {getApeArticlePage,removeApeArticle,editApeArticle} from '../../../api/api' \r\nimport add from '../../../components/system/article/addArticle'\r\nimport update from '../../../components/system/article/updateArticle'\r\nexport default {\r\n  data() {\r\n    return{\r\n      loading: true,\r\n      update: [],\r\n      remove: [],\r\n      updateId: \"\",\r\n      addVisible: false,\r\n      updateVisible: false,\r\n      taskId: \"\",\r\n      type: 1,\r\n      search: {\r\n          title: \"\",\r\n          state: \"\",\r\n          taskName: \"\",\r\n          pageNumber: 1,\r\n          pageSize:10,\r\n          type: 1\r\n      },\r\n      total: 0,\r\n      tableData: []\r\n    }\r\n  },\r\n  components: {\r\n    add,\r\n    update\r\n  },\r\n  methods: {\r\n    tableHeaderStyle() {\r\n      return {\r\n        'color': '#4A2B90',\r\n        'background-color': '#ECE9F4',\r\n        'font-weight': '600',\r\n        'border-bottom': '1px solid #D8D0F0',\r\n        'font-family': 'Microsoft YaHei'\r\n      }\r\n    },\r\n    tableRowStyle() {\r\n      return {\r\n        'color': '#5F4B8B',\r\n        'font-size': '14px',\r\n        'font-family': 'Microsoft YaHei',\r\n        'border-bottom': '1px solid #F0EEF7'\r\n      }\r\n    },\r\n    toArticleComment(id) {\r\n      var param = {\r\n          \"name\": \"文章评论\",\r\n          \"url\": \"/articleComment?id=\" + id\r\n      }\r\n      this.$store.commit('menu/addActiveMenu', param)\r\n      this.$router.push(\"/articleComment?id=\" + id)\r\n      this.$store.commit('menu/setActiveMenu', \"/articleComment?id=\" + id)\r\n    },\r\n    listing(id) {\r\n      var param = {\r\n        id: id,\r\n        state: 0\r\n      }\r\n      editApeArticle(param).then(res => {\r\n        if(res.code == 1000) {\r\n          this.$notify.success({\r\n            title: '成功',\r\n            message: \"审核成功\"\r\n          });\r\n          this.query();\r\n        } else {\r\n          this.$notify.error({\r\n            title: '错误',\r\n            message: res.message\r\n          });\r\n        }\r\n      })\r\n    },\r\n    offShelf(id) {\r\n      var param = {\r\n        id: id,\r\n        state: 1\r\n      }\r\n      editApeArticle(param).then(res => {\r\n        if(res.code == 1000) {\r\n          this.$notify.success({\r\n            title: '成功',\r\n            message: \"审核成功\"\r\n          });\r\n          this.query()\r\n        } else {\r\n          this.$notify.error({\r\n            title: '错误',\r\n            message: res.message\r\n          });\r\n        }\r\n      })\r\n    },\r\n    searchPage() {\r\n      this.search.pageNumber = 1\r\n      this.query()\r\n    },\r\n    query() {\r\n      getApeArticlePage(this.search).then(res => {\r\n        if(res.code == 1000) {\r\n          this.tableData = res.data.records\r\n          this.total = res.data.total\r\n          this.loading = false\r\n        } else {\r\n          this.$notify.error({\r\n            title: '错误',\r\n            message: res.message\r\n          });\r\n        }\r\n      })\r\n    },\r\n    refresh() {\r\n      this.search.title = \"\"\r\n      this.search.state = \"\"\r\n      this.search.taskName = \"\"\r\n      this.query()\r\n    },\r\n    handleCurrentChange(val) {\r\n      this.search.pageNumber = val\r\n      this.query()\r\n    },\r\n    handleSizeChange(val) {\r\n      this.search.pageSize = val\r\n      this.query()\r\n    },\r\n    handleSelectionChange(val) {\r\n      this.update = []\r\n      this.remove = []\r\n      for (let i = 0;i < val.length;i++) {\r\n        var item = val[i]\r\n        this.update.push(item.id)\r\n        this.remove.push(item.id)\r\n      }\r\n    },\r\n    add() {\r\n      this.addVisible = true\r\n    },\r\n    addFalse() {\r\n      this.addVisible = false\r\n      this.query()\r\n    },\r\n    updateFalse() {\r\n      this.updateId = ''\r\n      this.updateVisible = false\r\n      this.query()\r\n    },\r\n    updateData(id) {\r\n      this.updateId = id\r\n      this.updateVisible = true\r\n    },\r\n    updateDataBtn() {\r\n      this.updateData(this.update[0])\r\n    },\r\n    deleteDataBtn() {\r\n      this.$confirm('确定删除选中的'+ this.remove.length +'条数据?', '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning',\r\n        customClass: 'purple-message-box'\r\n      }).then(() => {\r\n        this.deleteDate(this.remove.join(\",\"))\r\n      }).catch(() => {});\r\n    },\r\n    deleteDate(ids) {\r\n      removeApeArticle({ids:ids}).then(res => {\r\n          if(res.code == 1000) {\r\n            this.$message({\r\n              type: 'success',\r\n              message: '删除成功!',\r\n              customClass: 'purple-message'\r\n            });\r\n            this.pageNumber = 1\r\n            this.query()\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n  