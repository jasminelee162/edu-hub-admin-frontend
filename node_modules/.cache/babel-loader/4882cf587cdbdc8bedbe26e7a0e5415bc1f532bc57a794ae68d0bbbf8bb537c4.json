{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getMenuList, removeMenu } from '../../../api/api';\nimport addMenu from '../../../components/system/menu/addMenu';\nimport updateMenu from '../../../components/system/menu/updateMenu';\nexport default {\n  data() {\n    return {\n      loading: true,\n      refreshTable: true,\n      expand: true,\n      addMenuVisible: false,\n      updateMenuVisible: false,\n      search: {\n        menuName: \"\",\n        status: \"\"\n      },\n      tableData: [],\n      parentId: [],\n      disable: false,\n      updateId: \"\"\n    };\n  },\n  components: {\n    addMenu,\n    updateMenu\n  },\n  methods: {\n    tableHeaderStyle() {\n      return {\n        'color': '#4A2B90',\n        'background-color': '#ECE9F4',\n        'font-weight': 'bold',\n        'border-bottom': '1px solid #7B68EE'\n      };\n    },\n    tableRowStyle() {\n      return {\n        'color': '#5F4B8B',\n        'font-size': '14px',\n        'border-bottom': '1px solid #F0EEF7'\n      };\n    },\n    query() {\n      getMenuList(this.search).then(res => {\n        if (res.code == 1000) {\n          this.tableData = res.data;\n          this.loading = false;\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    refresh() {\n      this.search.menuName = \"\";\n      this.search.status = \"\";\n      this.query();\n    },\n    changeExpand() {\n      this.refreshTable = false;\n      this.expand = !this.expand;\n      this.$nextTick(() => {\n        this.refreshTable = true;\n      });\n    },\n    addMenu() {\n      this.addMenuVisible = true;\n    },\n    addMenuFalse() {\n      this.addMenuVisible = false;\n      this.parentId = [];\n      this.disable = false;\n      this.query();\n    },\n    updateMenu() {\n      this.updateMenuVisible = true;\n    },\n    updateMenuFalse() {\n      this.updateId = \"\";\n      this.updateMenuVisible = false;\n      this.query();\n    },\n    handleCommand(command) {\n      var data = command.split(\"#\");\n      if (data[1] == 'add') {\n        var idArrary = data[0].split(\",\");\n        var id = data[2];\n        if (idArrary[0] != \"0\") {\n          this.parentId = idArrary;\n        }\n        this.parentId.push(id);\n        this.disable = true;\n        this.addMenu();\n      } else if (data[1] == 'edit') {\n        this.updateId = data[0];\n        this.updateMenu();\n      } else {\n        this.deleteDate(data[0]);\n      }\n    },\n    deleteDate(id) {\n      this.$confirm('确定删除选中的数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        customClass: 'tech-message-box'\n      }).then(() => {\n        removeMenu({\n          id: id\n        }).then(res => {\n          if (res.code == 1000) {\n            this.$message({\n              type: 'success',\n              message: '删除成功!',\n              customClass: 'tech-message'\n            });\n            this.query();\n          } else {\n            this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }).catch(() => {});\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["getMenuList","removeMenu","addMenu","updateMenu","data","loading","refreshTable","expand","addMenuVisible","updateMenuVisible","search","menuName","status","tableData","parentId","disable","updateId","components","methods","tableHeaderStyle","tableRowStyle","query","then","res","code","$notify","error","title","message","refresh","changeExpand","$nextTick","addMenuFalse","updateMenuFalse","handleCommand","command","split","idArrary","id","push","deleteDate","$confirm","confirmButtonText","cancelButtonText","type","customClass","$message","catch","mounted"],"sources":["src/views/system/menu/menu.vue"],"sourcesContent":["<template>\r\n  <div class=\"menu-management\">\r\n    <div class=\"search-panel\">\r\n      <el-row :gutter=\"15\">\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-menu\"></i> 菜单名称:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入菜单名称\"\r\n              v-model=\"search.menuName\"\r\n              class=\"tech-input\">\r\n              <i slot=\"prefix\" class=\"el-icon-edit-outline\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-title\">\r\n              <i class=\"el-icon-circle-check\"></i> 菜单状态:\r\n            </span>\r\n            <el-select \r\n              clearable \r\n              size=\"small\" \r\n              v-model=\"search.status\" \r\n              placeholder=\"请选择\"\r\n              class=\"tech-select\">\r\n              <el-option\r\n                label=\"正常\"\r\n                value=\"0\">\r\n                <span class=\"status-option\">\r\n                  <i class=\"el-icon-success\" style=\"color:#67C23A\"></i> 正常\r\n                </span>\r\n              </el-option>\r\n              <el-option\r\n                label=\"停用\"\r\n                value=\"1\">\r\n                <span class=\"status-option\">\r\n                  <i class=\"el-icon-error\" style=\"color:#F56C6C\"></i> 停用\r\n                </span>\r\n              </el-option>\r\n            </el-select>\r\n          </div>\r\n        </el-col>\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n          <div class=\"search-actions\">\r\n            <el-button \r\n              size=\"small\" \r\n              icon=\"el-icon-search\" \r\n              type=\"primary\" \r\n              @click=\"query\"\r\n              class=\"search-btn\">\r\n              查询\r\n            </el-button>\r\n            <el-button \r\n              size=\"small\" \r\n              icon=\"el-icon-refresh\" \r\n              @click=\"refresh\"\r\n              class=\"reset-btn\">\r\n              重置\r\n            </el-button>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <div class=\"data-panel\">\r\n      <div class=\"action-bar\">\r\n        <el-button \r\n          type=\"primary\" \r\n          size=\"small\" \r\n          icon=\"el-icon-plus\" \r\n          @click=\"addMenu\"\r\n          class=\"action-btn add-btn\">\r\n          新增\r\n        </el-button>\r\n        <el-button \r\n          type=\"info\" \r\n          size=\"small\" \r\n          icon=\"el-icon-sort\" \r\n          @click=\"changeExpand\"\r\n          class=\"action-btn expand-btn\">\r\n          {{ expand ? '折叠' : '展开' }}\r\n        </el-button>\r\n      </div>\r\n\r\n      <el-table\r\n        v-if=\"refreshTable\"\r\n        v-loading=\"loading\"\r\n        :data=\"tableData\"\r\n        :header-cell-style=\"tableHeaderStyle\"\r\n        :row-style=\"tableRowStyle\"\r\n        style=\"width: 100%;margin-bottom: 20px;\"\r\n        row-key=\"id\"\r\n        :default-expand-all=\"expand\"\r\n        :tree-props=\"{children: 'children', hasChildren: 'hasChildren'}\"\r\n        class=\"tech-table\">\r\n        <el-table-column\r\n          prop=\"menuName\"\r\n          label=\"菜单名称\"\r\n          min-width=\"200\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"name-cell\">\r\n              <i :class=\"row.icon || 'el-icon-document'\" style=\"color:#7B68EE; margin-right:8px\"></i>\r\n              <span>{{ row.menuName }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"icon\"\r\n          label=\"图标\"\r\n          width=\"120\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"icon-cell\">\r\n              <i :class=\"row.icon || 'el-icon-question'\" style=\"color:#9370DB\"></i>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"orderNum\"\r\n          label=\"排序\"\r\n          width=\"100\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"order-cell\">\r\n              <i class=\"el-icon-sort\" style=\"color:#A5A4BF; margin-right:5px\"></i>\r\n              <span>{{ row.orderNum }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"perms\"\r\n          label=\"权限标识\"\r\n          min-width=\"180\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"perms-cell\">\r\n              <i class=\"el-icon-key\" style=\"color:#A5A4BF; margin-right:5px\"></i>\r\n              <span>{{ row.perms || '-' }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"componentUrl\"\r\n          label=\"组件路径\"\r\n          min-width=\"200\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"path-cell\">\r\n              <i class=\"el-icon-link\" style=\"color:#A5A4BF; margin-right:5px\"></i>\r\n              <span>{{ row.componentUrl || '-' }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"status\"\r\n          label=\"状态\"\r\n          width=\"120\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"status-cell\">\r\n              <el-tag \r\n                v-if=\"row.status == 0\" \r\n                type=\"success\"\r\n                effect=\"dark\">\r\n                <i class=\"el-icon-success\"></i> 正常\r\n              </el-tag>\r\n              <el-tag \r\n                v-if=\"row.status == 1\" \r\n                type=\"warning\"\r\n                effect=\"dark\">\r\n                <i class=\"el-icon-error\"></i> 停用\r\n              </el-tag>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          prop=\"createTime\"\r\n          label=\"创建时间\"\r\n          width=\"180\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"time-cell\">\r\n              <i class=\"el-icon-time\" style=\"color:#A5A4BF; margin-right:5px\"></i>\r\n              <span>{{ row.createTime }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        <el-table-column\r\n          label=\"操作\"\r\n          width=\"180\"\r\n          fixed=\"right\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"action-buttons\">\r\n              <el-dropdown \r\n                trigger=\"click\" \r\n                @command=\"handleCommand\"\r\n                placement=\"bottom-end\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  class=\"action-btn more-btn\">\r\n                  <i class=\"el-icon-more\"></i> 操作\r\n                </el-button>\r\n                <el-dropdown-menu slot=\"dropdown\" class=\"tech-dropdown\">\r\n                  <el-dropdown-item \r\n                    icon=\"el-icon-plus\" \r\n                    :command=\"row.idArrary + '#add#' + row.id\"\r\n                    class=\"dropdown-item\">\r\n                    新增\r\n                  </el-dropdown-item>\r\n                  <el-dropdown-item \r\n                    icon=\"el-icon-edit\" \r\n                    :command=\"row.id + '#edit'\"\r\n                    class=\"dropdown-item\">\r\n                    修改\r\n                  </el-dropdown-item>\r\n                  <el-dropdown-item \r\n                    icon=\"el-icon-delete\" \r\n                    :command=\"row.id + '#remove'\"\r\n                    class=\"dropdown-item delete-item\">\r\n                    删除\r\n                  </el-dropdown-item>\r\n                </el-dropdown-menu>\r\n              </el-dropdown>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n    </div>\r\n    \r\n    <add-menu @addMenuFalse=\"addMenuFalse\" :disable=\"disable\" :parentId=\"parentId\" :dropData=\"tableData\" :addMenuVisible=\"addMenuVisible\"></add-menu>\r\n    <update-menu @updateMenuFalse=\"updateMenuFalse\" :updateId=\"updateId\" :dropData=\"tableData\" :updateMenuVisible=\"updateMenuVisible\"></update-menu>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\n  import {getMenuList,removeMenu} from '../../../api/api' \r\n  import addMenu from '../../../components/system/menu/addMenu'\r\n  import updateMenu from '../../../components/system/menu/updateMenu'\r\n  export default {\r\n    data() {\r\n      return{\r\n        loading: true,\r\n        refreshTable: true,\r\n        expand:true,\r\n        addMenuVisible: false,\r\n        updateMenuVisible: false,\r\n        search: {\r\n            menuName: \"\",\r\n            status: \"\",\r\n        },\r\n        tableData: [],\r\n        parentId: [],\r\n        disable: false,\r\n        updateId: \"\",\r\n      }\r\n    },\r\n    components: {\r\n      addMenu,\r\n      updateMenu\r\n    },\r\n    methods: {\r\n      tableHeaderStyle() {\r\n        return {\r\n          'color': '#4A2B90',\r\n          'background-color': '#ECE9F4',\r\n          'font-weight': 'bold',\r\n          'border-bottom': '1px solid #7B68EE'\r\n        }\r\n      },\r\n      tableRowStyle() {\r\n        return {\r\n          'color': '#5F4B8B',\r\n          'font-size': '14px',\r\n          'border-bottom': '1px solid #F0EEF7'\r\n        }\r\n      },\r\n      query() {\r\n        getMenuList(this.search).then(res => {\r\n          if(res.code == 1000) {\r\n            this.tableData = res.data\r\n            this.loading = false\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            });\r\n          }\r\n        })\r\n      },\r\n      refresh() {\r\n        this.search.menuName = \"\"\r\n        this.search.status = \"\"\r\n        this.query()\r\n      },\r\n      changeExpand() {\r\n        this.refreshTable = false;\r\n        this.expand = !this.expand;\r\n        this.$nextTick(() => {\r\n          this.refreshTable = true;\r\n        });\r\n      },\r\n      addMenu() {\r\n        this.addMenuVisible = true\r\n      },\r\n      addMenuFalse() {\r\n        this.addMenuVisible = false\r\n        this.parentId = []\r\n        this.disable = false\r\n        this.query()\r\n      },\r\n      updateMenu() {\r\n        this.updateMenuVisible = true\r\n      },\r\n      updateMenuFalse() {\r\n        this.updateId = \"\"\r\n        this.updateMenuVisible = false\r\n        this.query()\r\n      },\r\n      handleCommand(command) {\r\n        var data = command.split(\"#\")\r\n        if(data[1] == 'add') {\r\n          var idArrary = data[0].split(\",\")\r\n          var id = data[2]\r\n          if (idArrary[0] != \"0\") {\r\n            this.parentId = idArrary\r\n          }\r\n          this.parentId.push(id)\r\n          this.disable = true\r\n          this.addMenu()\r\n        } else if (data[1] == 'edit') {\r\n          this.updateId = data[0]\r\n          this.updateMenu()\r\n        } else {\r\n          this.deleteDate(data[0])\r\n        }\r\n      },\r\n      deleteDate(id) {\r\n        this.$confirm('确定删除选中的数据?', '提示', {\r\n          confirmButtonText: '确定',\r\n          cancelButtonText: '取消',\r\n          type: 'warning',\r\n          customClass: 'tech-message-box'\r\n        }).then(() => {\r\n          removeMenu({id:id}).then(res => {\r\n            if(res.code == 1000) {\r\n              this.$message({\r\n                type: 'success',\r\n                message: '删除成功!',\r\n                customClass: 'tech-message'\