{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport { getMenuList, removeMenu } from '../../../api/api';\nimport addMenu from '../../../components/system/menu/addMenu';\nimport updateMenu from '../../../components/system/menu/updateMenu';\nexport default {\n  data() {\n    return {\n      loading: true,\n      refreshTable: true,\n      expand: true,\n      addMenuVisible: false,\n      updateMenuVisible: false,\n      search: {\n        menuName: \"\",\n        status: \"\"\n      },\n      tableData: [],\n      parentId: [],\n      disable: false,\n      updateId: \"\"\n    };\n  },\n  components: {\n    addMenu,\n    updateMenu\n  },\n  methods: {\n    tableHeaderStyle() {\n      return {\n        'color': '#4A2B90',\n        'background-color': '#ECE9F4',\n        'font-weight': 'bold',\n        'border-bottom': '1px solid #7B68EE'\n      };\n    },\n    tableRowStyle() {\n      return {\n        'color': '#5F4B8B',\n        'font-size': '14px',\n        'border-bottom': '1px solid #F0EEF7'\n      };\n    },\n    query() {\n      getMenuList(this.search).then(res => {\n        if (res.code == 1000) {\n          this.tableData = res.data;\n          this.loading = false;\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    },\n    refresh() {\n      this.search.menuName = \"\";\n      this.search.status = \"\";\n      this.query();\n    },\n    changeExpand() {\n      this.refreshTable = false;\n      this.expand = !this.expand;\n      this.$nextTick(() => {\n        this.refreshTable = true;\n      });\n    },\n    addMenu() {\n      this.addMenuVisible = true;\n    },\n    addMenuFalse() {\n      this.addMenuVisible = false;\n      this.parentId = [];\n      this.disable = false;\n      this.query();\n    },\n    updateMenu() {\n      this.updateMenuVisible = true;\n    },\n    updateMenuFalse() {\n      this.updateId = \"\";\n      this.updateMenuVisible = false;\n      this.query();\n    },\n    handleCommand(command) {\n      var data = command.split(\"#\");\n      if (data[1] == 'add') {\n        var idArrary = data[0].split(\",\");\n        var id = data[2];\n        if (idArrary[0] != \"0\") {\n          this.parentId = idArrary;\n        }\n        this.parentId.push(id);\n        this.disable = true;\n        this.addMenu();\n      } else if (data[1] == 'edit') {\n        this.updateId = data[0];\n        this.updateMenu();\n      } else {\n        this.deleteDate(data[0]);\n      }\n    },\n    deleteDate(id) {\n      this.$confirm('确定删除选中的数据?', '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        customClass: 'tech-message-box'\n      }).then(() => {\n        removeMenu({\n          id: id\n        }).then(res => {\n          if (res.code == 1000) {\n            this.$message({\n              type: 'success',\n              message: '删除成功!',\n              customClass: 'tech-message'\n            });\n            this.query();\n          } else {\n            this.$notify.error({\n              title: '错误',\n              message: res.message\n            });\n          }\n        });\n      }).catch(() => {});\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["getMenuList","removeMenu","addMenu","updateMenu","data","loading","refreshTable","expand","addMenuVisible","updateMenuVisible","search","menuName","status","tableData","parentId","disable","updateId","components","methods","tableHeaderStyle","tableRowStyle","query","then","res","code","$notify","error","title","message","refresh","changeExpand","$nextTick","addMenuFalse","updateMenuFalse","handleCommand","command","split","idArrary","id","push","deleteDate","$confirm","confirmButtonText","cancelButtonText","type","customClass","$message","catch","mounted"],"sources":["src/views/system/menu/menu.vue"],"sourcesContent":["<template>\n  <div class=\"menu-management\">\n    <div class=\"search-panel\">\n      <el-row :gutter=\"15\">\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\n          <div class=\"search-item\">\n            <span class=\"search-title\">\n              <i class=\"el-icon-menu\"></i> 菜单名称:\n            </span>\n            <el-input\n                size=\"small\"\n                placeholder=\"请输入菜单名称\"\n                v-model=\"search.menuName\"\n                class=\"tech-input\">\n              <i slot=\"prefix\" class=\"el-icon-edit-outline\"></i>\n            </el-input>\n          </div>\n        </el-col>\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\n          <div class=\"search-item\">\n            <span class=\"search-title\">\n              <i class=\"el-icon-circle-check\"></i> 菜单状态:\n            </span>\n            <el-select\n                clearable\n                size=\"small\"\n                v-model=\"search.status\"\n                placeholder=\"请选择\"\n                class=\"tech-select\">\n              <el-option\n                  label=\"正常\"\n                  value=\"0\">\n                <span class=\"status-option\">\n                  <i class=\"el-icon-success\" style=\"color:#67C23A\"></i> 正常\n                </span>\n              </el-option>\n              <el-option\n                  label=\"停用\"\n                  value=\"1\">\n                <span class=\"status-option\">\n                  <i class=\"el-icon-error\" style=\"color:#F56C6C\"></i> 停用\n                </span>\n              </el-option>\n            </el-select>\n          </div>\n        </el-col>\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\n          <div class=\"search-actions\">\n            <el-button\n                size=\"small\"\n                icon=\"el-icon-search\"\n                type=\"primary\"\n                @click=\"query\"\n                class=\"search-btn\">\n              查询\n            </el-button>\n            <el-button\n                size=\"small\"\n                icon=\"el-icon-refresh\"\n                @click=\"refresh\"\n                class=\"reset-btn\">\n              重置\n            </el-button>\n          </div>\n        </el-col>\n      </el-row>\n    </div>\n\n    <div class=\"data-panel\">\n      <div class=\"action-bar\">\n        <!--<el-button\n            type=\"primary\"\n            size=\"small\"\n            icon=\"el-icon-plus\"\n            @click=\"addMenu\"\n            class=\"action-btn add-btn\">\n          新增\n        </el-button>-->\n        <el-button\n            type=\"info\"\n            size=\"small\"\n            icon=\"el-icon-sort\"\n            @click=\"changeExpand\"\n            class=\"action-btn expand-btn\">\n          {{ expand ? '折叠' : '展开' }}\n        </el-button>\n      </div>\n\n      <el-table\n          v-if=\"refreshTable\"\n          v-loading=\"loading\"\n          :data=\"tableData\"\n          :header-cell-style=\"tableHeaderStyle\"\n          :row-style=\"tableRowStyle\"\n          style=\"width: 100%;margin-bottom: 20px;\"\n          row-key=\"id\"\n          :default-expand-all=\"expand\"\n          :tree-props=\"{children: 'children', hasChildren: 'hasChildren'}\"\n          class=\"tech-table\">\n        <el-table-column\n            prop=\"menuName\"\n            label=\"菜单名称\"\n            min-width=\"200\">\n          <template #default=\"{row}\">\n            <div class=\"name-cell\">\n              <i :class=\"row.icon || 'el-icon-document'\" style=\"color:#7B68EE; margin-right:8px\"></i>\n              <span>{{ row.menuName }}</span>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column\n            prop=\"icon\"\n            label=\"图标\"\n            width=\"120\">\n          <template #default=\"{row}\">\n            <div class=\"icon-cell\">\n              <i :class=\"row.icon || 'el-icon-question'\" style=\"color:#9370DB\"></i>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column\n            prop=\"orderNum\"\n            label=\"排序\"\n            width=\"100\">\n          <template #default=\"{row}\">\n            <div class=\"order-cell\">\n              <i class=\"el-icon-sort\" style=\"color:#A5A4BF; margin-right:5px\"></i>\n              <span>{{ row.orderNum }}</span>\n            </div>\n          </template>\n        </el-table-column>\n        <!--<el-table-column\n            prop=\"perms\"\n            label=\"权限标识\"\n            min-width=\"180\">\n          <template #default=\"{row}\">\n            <div class=\"perms-cell\">\n              <i class=\"el-icon-key\" style=\"color:#A5A4BF; margin-right:5px\"></i>\n              <span>{{ row.perms || '-' }}</span>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column\n            prop=\"componentUrl\"\n            label=\"组件路径\"\n            min-width=\"200\">\n          <template #default=\"{row}\">\n            <div class=\"path-cell\">\n              <i class=\"el-icon-link\" style=\"color:#A5A4BF; margin-right:5px\"></i>\n              <span>{{ row.componentUrl || '-' }}</span>\n            </div>\n          </template>\n        </el-table-column>-->\n        <el-table-column\n            prop=\"status\"\n            label=\"状态\"\n            width=\"120\">\n          <template #default=\"{row}\">\n            <div class=\"status-cell\">\n              <el-tag\n                  v-if=\"row.status == 0\"\n                  type=\"success\"\n                  effect=\"dark\">\n                <i class=\"el-icon-success\"></i> 正常\n              </el-tag>\n              <el-tag\n                  v-if=\"row.status == 1\"\n                  type=\"warning\"\n                  effect=\"dark\">\n                <i class=\"el-icon-error\"></i> 停用\n              </el-tag>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column\n            prop=\"createTime\"\n            label=\"创建时间\"\n            width=\"180\">\n          <template #default=\"{row}\">\n            <div class=\"time-cell\">\n              <i class=\"el-icon-time\" style=\"color:#A5A4BF; margin-right:5px\"></i>\n              <span>{{ row.createTime }}</span>\n            </div>\n          </template>\n        </el-table-column>\n        <el-table-column\n            label=\"操作\"\n            width=\"180\"\n            fixed=\"right\">\n          <template #default=\"{row}\">\n            <div class=\"action-buttons\">\n              <el-dropdown\n                  trigger=\"click\"\n                  @command=\"handleCommand\"\n                  placement=\"bottom-end\">\n                <el-button\n                    size=\"mini\"\n                    class=\"action-btn more-btn\">\n                  <i class=\"el-icon-more\"></i> 操作\n                </el-button>\n                <el-dropdown-menu slot=\"dropdown\" class=\"tech-dropdown\">\n                  <!--<el-dropdown-item\n                      icon=\"el-icon-plus\"\n                      :command=\"row.idArrary + '#add#' + row.id\"\n                      class=\"dropdown-item\">\n                    新增\n                  </el-dropdown-item>-->\n                  <el-dropdown-item\n                      icon=\"el-icon-edit\"\n                      :command=\"row.id + '#edit'\"\n                      class=\"dropdown-item\">\n                    修改\n                  </el-dropdown-item>\n                  <el-dropdown-item\n                      icon=\"el-icon-delete\"\n                      :command=\"row.id + '#remove'\"\n                      class=\"dropdown-item delete-item\">\n                    删除\n                  </el-dropdown-item>\n                </el-dropdown-menu>\n              </el-dropdown>\n            </div>\n          </template>\n        </el-table-column>\n      </el-table>\n    </div>\n\n    <add-menu @addMenuFa