Number = 1;\n          this.query();\n        } else {\n          this.$notify.error({\n            title: '错误',\n            message: res.message\n          });\n        }\n      });\n    }\n  },\n  mounted() {\n    var name = this.$route.query.name;\n    var id = this.$route.query.id;\n    var type = this.$route.query.type;\n    if (name) {\n      this.search.taskName = name;\n    }\n    if (id) {\n      this.taskId = id;\n    }\n    if (type) {\n      this.type = type;\n    }\n    this.query();\n  }\n};","map":{"version":3,"names":["getApeArticlePage","removeApeArticle","editApeArticle","add","update","data","loading","remove","updateId","addVisible","updateVisible","taskId","type","search","title","state","taskName","pageNumber","pageSize","total","tableData","components","methods","toArticleComment","id","param","$store","commit","$router","push","listing","then","res","code","$notify","success","message","query","error","offShelf","searchPage","records","refresh","handleCurrentChange","val","handleSizeChange","handleSelectionChange","i","length","item","addFalse","updateFalse","updateData","updateDataBtn","deleteDataBtn","$confirm","confirmButtonText","cancelButtonText","deleteDate","join","catch","ids","$message","mounted","name","$route"],"sources":["src/views/system/article/teacherArticle.vue"],"sourcesContent":["<template>\r\n  <div class=\"_article\">\r\n      <div class=\"search-table\">\r\n        <div class=\"search\">\r\n            <el-row :gutter=\"10\" style=\"padding:10px\">\r\n                <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"8\" :xl=\"8\">\r\n                    <span class=\"search-title\">课程名称:</span>\r\n                    <el-input\r\n                        :disabled=\"type == 0? true:false\"\r\n                        style=\"margin-top:10px\"\r\n                        size=\"mini\"\r\n                        placeholder=\"请输入课程名称\"\r\n                        v-model=\"search.taskName\">\r\n                    </el-input>\r\n 