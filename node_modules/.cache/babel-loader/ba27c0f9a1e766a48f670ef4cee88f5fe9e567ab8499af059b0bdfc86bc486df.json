{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport { getForumPage, removeForum } from '@/api/forum';\nimport { formatDateTime } from '@/utils/date';\nexport default {\n  data() {\n    return {\n      loading: true,\n      selected: [],\n      search: {\n        name: \"\",\n        content: \"\",\n        pageNumber: 1,\n        pageSize: 10\n      },\n      total: 0,\n      tableData: []\n    };\n  },\n  methods: {\n    formatTime(time) {\n      return formatDateTime(time);\n    },\n    tableHeaderStyle() {\n      return {\n        'color': '#4A2B90',\n        'background-color': '#F5F3FF',\n        'font-weight': '600',\n        'border-bottom': '1px solid #D8D0F0'\n      };\n    },\n    tableRowStyle() {\n      return {\n        'color': '#5F4B8B',\n        'font-size': '14px',\n        'border-bottom': '1px solid #F0ECFF'\n      };\n    },\n    viewDetail(id) {\n      this.$router.push({\n        path: '/forum/detail',\n        query: {\n          id\n        }\n      });\n    },\n    searchPage() {\n      this.search.pageNumber = 1;\n      this.query();\n    },\n    async query() {\n      try {\n        this.loading = true;\n        const {\n          code,\n          data,\n          message\n        } = await getForumPage(this.search);\n        if (code === 1000) {\n          this.tableData = data.records || [];\n          this.total = data.total || 0;\n        } else {\n          this.$message.error(message || '获取数据失败');\n        }\n      } catch (error) {\n        console.error('获取讨论列表失败:', error);\n        this.$message.error('获取数据失败，请稍后重试');\n      } finally {\n        this.loading = false;\n      }\n    },\n    refresh() {\n      this.search = {\n        name: \"\",\n        content: \"\",\n        pageNumber: 1,\n        pageSize: 10\n      };\n      this.query();\n    },\n    handleCurrentChange(val) {\n      this.search.pageNumber = val;\n      this.query();\n    },\n    handleSizeChange(val) {\n      this.search.pageSize = val;\n      this.query();\n    },\n    handleSelectionChange(selection) {\n      this.selected = selection.map(item => item.id);\n    },\n    batchDelete() {\n      if (!this.selected.length) {\n        this.$message.warning('请至少选择一条记录');\n        return;\n      }\n      this.$confirm(`确定删除选中的 ${this.selected.length} 条讨论吗？`, '提示', {\n        confirmButtonText: '确定',\n        cancelButtonText: '取消',\n        type: 'warning',\n        customClass: 'forum-message-box',\n        beforeClose: (action, instance, done) => {\n          if (action === 'confirm') {\n            instance.confirmButtonLoading = true;\n            this.deleteItems(this.selected.join(',')).finally(() => {\n              instance.confirmButtonLoading = false;\n              done();\n            });\n          } else {\n            done();\n          }\n        }\n      }).catch(() => {\n        this.$message.info('已取消删除');\n      });\n    },\n    async deleteItems(ids) {\n      try {\n        const {\n          code,\n          message\n        } = await removeForum({\n          ids\n        });\n        if (code === 1000) {\n          this.$message.success('删除成功');\n          this.query();\n        } else {\n          this.$message.error(message || '删除失败');\n        }\n      } catch (error) {\n        console.error('删除讨论失败:', error);\n        this.$message.error('删除失败，请稍后重试');\n      }\n    },\n    deleteItem(id) {\n      this.deleteItems(id);\n    }\n  },\n  mounted() {\n    this.query();\n  }\n};","map":{"version":3,"names":["getForumPage","removeForum","formatDateTime","data","loading","selected","search","name","content","pageNumber","pageSize","total","tableData","methods","formatTime","time","tableHeaderStyle","tableRowStyle","viewDetail","id","$router","push","path","query","searchPage","code","message","records","$message","error","console","refresh","handleCurrentChange","val","handleSizeChange","handleSelectionChange","selection","map","item","batchDelete","length","warning","$confirm","confirmButtonText","cancelButtonText","type","customClass","beforeClose","action","instance","done","confirmButtonLoading","deleteItems","join","finally","catch","info","ids","success","deleteItem","mounted"],"sources":["src/views/system/user/user.vue"],"sourcesContent":["<template>\r\n  <div class=\"forum-management-container\">\r\n    <!-- 搜索面板 -->\r\n    <div class=\"search-panel\">\r\n      <el-row :gutter=\"20\">\r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\" :xl=\"6\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-label\">\r\n              <i class=\"el-icon-chat-line-square\"></i> 讨论名称:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入讨论名称\"\r\n              v-model=\"search.name\"\r\n              class=\"forum-input\"\r\n              clearable>\r\n              <i slot=\"prefix\" class=\"el-icon-search\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"12\" :md=\"8\" :lg=\"6\" :xl=\"6\">\r\n          <div class=\"search-item\">\r\n            <span class=\"search-label\">\r\n              <i class=\"el-icon-document\"></i> 讨论内容:\r\n            </span>\r\n            <el-input\r\n              size=\"small\"\r\n              placeholder=\"请输入讨论内容\"\r\n              v-model=\"search.content\"\r\n              class=\"forum-input\"\r\n              clearable>\r\n              <i slot=\"prefix\" class=\"el-icon-tickets\"></i>\r\n            </el-input>\r\n          </div>\r\n        </el-col>\r\n        \r\n        <el-col :xs=\"24\" :sm=\"24\" :md=\"8\" :lg=\"12\" :xl=\"12\">\r\n          <div class=\"action-buttons\">\r\n            <el-button \r\n              type=\"primary\" \r\n              size=\"small\" \r\n              icon=\"el-icon-search\"\r\n              @click=\"searchPage\"\r\n              class=\"search-action-btn\">\r\n              查询\r\n            </el-button>\r\n            <el-button \r\n              type=\"info\" \r\n              size=\"small\" \r\n              icon=\"el-icon-refresh-left\"\r\n              @click=\"refresh\"\r\n              class=\"reset-action-btn\">\r\n              重置\r\n            </el-button>\r\n          </div>\r\n        </el-col>\r\n      </el-row>\r\n    </div>\r\n\r\n    <!-- 数据面板 -->\r\n    <div class=\"data-panel\">\r\n      <!-- 操作工具栏 -->\r\n      <div class=\"tool-bar\">\r\n        <el-button \r\n          type=\"danger\" \r\n          size=\"small\" \r\n          icon=\"el-icon-delete\" \r\n          :disabled=\"!selected.length\"\r\n          @click=\"batchDelete\"\r\n          class=\"batch-delete-btn\">\r\n          批量删除\r\n        </el-button>\r\n      </div>\r\n      \r\n      <!-- 数据表格 -->\r\n      <el-table\r\n        v-loading=\"loading\"\r\n        :data=\"tableData\"\r\n        :header-cell-style=\"tableHeaderStyle\"\r\n        :row-style=\"tableRowStyle\"\r\n        @selection-change=\"handleSelectionChange\"\r\n        stripe\r\n        border\r\n        class=\"forum-table\"\r\n        style=\"width: 100%\">\r\n        \r\n        <el-table-column \r\n          type=\"selection\" \r\n          width=\"48\"\r\n          align=\"center\">\r\n        </el-table-column>\r\n        \r\n        <el-table-column \r\n          prop=\"name\" \r\n          label=\"讨论名称\"\r\n          min-width=\"220\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"discussion-name\">\r\n              <el-tag size=\"small\" effect=\"plain\" type=\"info\">\r\n                <i class=\"el-icon-chat-line-round\"></i>\r\n              </el-tag>\r\n              <span class=\"name-text\">{{ row.name }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column \r\n          prop=\"remark\" \r\n          label=\"备注\"\r\n          min-width=\"200\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"discussion-remark\">\r\n              <el-tag v-if=\"row.remark\" size=\"mini\" type=\"info\">\r\n                <i class=\"el-icon-collection-tag\"></i>\r\n                {{ row.remark }}\r\n              </el-tag>\r\n              <span v-else class=\"no-remark\">无备注</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column \r\n          prop=\"createBy\" \r\n          label=\"创建者\"\r\n          width=\"150\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"creator-info\">\r\n              <el-avatar :size=\"24\" :src=\"row.avatar || require('@/assets/default-avatar.png')\"></el-avatar>\r\n              <span class=\"creator-name\">{{ row.createBy }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column \r\n          prop=\"createTime\" \r\n          label=\"创建时间\"\r\n          width=\"180\"\r\n          sortable>\r\n          <template #default=\"{row}\">\r\n            <div class=\"create-time\">\r\n              <i class=\"el-icon-time\"></i>\r\n              <span>{{ formatTime(row.createTime) }}</span>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n        \r\n        <el-table-column \r\n          label=\"操作\" \r\n          width=\"160\"\r\n          fixed=\"right\">\r\n          <template #default=\"{row}\">\r\n            <div class=\"row-actions\">\r\n              <el-tooltip content=\"查看详情\" placement=\"top\">\r\n                <el-button \r\n                  size=\"mini\" \r\n                  circle\r\n                  @click=\"viewDetail(row.id)\"\r\n                  class=\"view-btn\">\r\n                  <i class=\"el-icon-view\"></i>\r\n                </el-button>\r\n              </el-tooltip>\r\n              \r\n              <el-popconfirm\r\n                title=\"确定要删除此讨论吗？\"\r\n                @confirm=\"deleteItem(row.id)\"\r\n                confirm-button-text=\"确认\"\r\n                cancel-button-text=\"取消\"\r\n                icon=\"el-icon-warning\"\r\n                icon-color=\"#ff4d4f\">\r\n                <el-tooltip content=\"删除\" placement=\"top\" slot=\"reference\">\r\n                  <el-button \r\n                    size=\"mini\" \r\n                    circle\r\n                    class=\"delete-btn\">\r\n                    <i class=\"el-icon-delete\"></i>\r\n                  </el-button>\r\n                </el-tooltip>\r\n              </el-popconfirm>\r\n            </div>\r\n          </template>\r\n        </el-table-column>\r\n      </el-table>\r\n      \r\n      <!-- 分页 -->\r\n      <div class=\"pagination-wrapper\">\r\n        <el-pagination\r\n          background\r\n          layout=\"total, sizes, prev, pager, next, jumper\"\r\n          :page-sizes=\"[10, 20, 50, 100]\"\r\n          :page-size=\"search.pageSize\"\r\n          :current-page=\"search.pageNumber\"\r\n          @size-change=\"handleSizeChange\"\r\n          @current-change=\"handleCurrentChange\"\r\n          :total=\"total\"\r\n          class=\"forum-pagination\">\r\n        </el-pagination>\r\n      </div>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nimport { getForumPage, removeForum } from '@/api/forum'\r\nimport { formatDateTime } from '@/utils/date'\r\n\r\nexport default {\r\n  data() {\r\n    return {\r\n      loading: true,\r\n      selected: [],\r\n      search: {\r\n        name: \"\",\r\n        content: \"\",\r\n        pageNumber: 1,\r\n        pageSize: 10\r\n      },\r\n      total: 0,\r\n      tableData: []\r\n    }\r\n  },\r\n  methods: {\r\n    formatTime(time) {\r\n      return formatDateTime(time)\r\n    },\r\n    \r\n    tableHeaderStyle() {\r\n      return {\r\n        'color': '#4A2B90',\r\n        'background-color': '#F5F3FF',\r\n        'font-weight': '600',\r\n        'border-bottom': '1px solid #D8D0F0'\r\n      }\r\n    },\r\n    \r\n    tableRowStyle() {\r\n      return {\r\n        'color': '#5F4B8B',\r\n        'font-size': '14px',\r\n        'border-bottom': '1px solid #F0ECFF'\r\n      }\r\n    },\r\n    \r\n    viewDetail(id) {\r\n      this.$router.push({\r\n        path: '/forum/detail',\r\n        query: { id }\r\n      })\r\n    },\r\n    \r\n    searchPage() {\r\n      this.search.pageNumber = 1\r\n      this.query()\r\n    },\r\n    \r\n    async query() {\r\n      try {\r\n        this.loading = true\r\n        const { code, data, message } = await getForumPage(this.search)\r\n        \r\n        if (code === 1000) {\r\n          this.tableData = data.records || []\r\n          this.total = data.total || 0\r\n        } else {\r\n          this.$message.error(message || '获取数据失败')\r\n        }\r\n      } catch (error) {\r\n        console.error('获取讨论列表失败:', error)\r\n        this.$message.error('获取数据失败，请稍后重试')\r\n      } finally {\r\n        this.loading = false\r\n      }\r\n    },\r\n    \r\n    refresh() {\r\n      this.search = {\r\n        name: \"\",\r\n        content: \"\",\r\n        pageNumber: 1,\r\n        pageSize: 10\r\n      }\r\n      this.query()\r\n    },\r\n    \r\n    handleCurrentChange(val) {\r\n      this.search.pageNumber = val\r\n      this.query()\r\n    },\r\n    \r\n    handleSizeChange(val) {\r\n      this.search.pageSize = val\r\n      this.query()\r\n    },\r\n    \r\n    handleSelectionChange(selection) {\r\n      this.selected = selection.map(item => item.id)\r\n    },\r\n    \r\n    batchDelete() {\r\n      if (!this.selected.length) {\r\n        this.$message.warning('请至少选择一条记录')\r\n        return\r\n      }\r\n      \r\n      this.$confirm(`确定删除选中的 ${this.selected.length} 条讨论吗？`, '提示', {\r\n        confirmButtonText: '确定',\r\n        cancelButtonText: '取消',\r\n        type: 'warning',\r\n        customClass: 'forum-message-box',\r\n        beforeClose: (action, instance, done) => {\r\n          if (action === 'confirm') {\r\n            instance.confirmButtonLoading = true\r\n            this.deleteItems(this.selected.join(','))\r\n              .finally(() => {\r\n                instance.confirmButtonLoading = false\r\n                done()\r\n              })\r\n          } else {\r\n            done()\r\n          }\r\n        }\r\n      }).catch(() => {\r\n        this.$message.info('已取消删除')\r\n      })\r\n    },\r\n    \r\n    async deleteItems(ids) {\r\n      try {\r\n        const { code, message } = await removeForum({ ids })\r\n        \r\n        if (code === 1000) {\r\n          this.$message.success('删除成功')\r\n          this.query()\r\n        } else {\r\n          this.$message.error(message || '删除失败')\r\n        }\r\n      } catch (error) {\r\n        console.error('删除讨论失败:', error)\r\n        this.$message.error('删除失败，请稍后重试')\r\n      }\r\n    },\r\n    \r\n    deleteItem(id) {\r\n      this.deleteItems(id)\r\n    }\r\n  },\r\n  \r\n  mounted() {\r\n    this.query()\r\n  }\r\n}\r\n</script>\r\n\r\n<style lang=\"scss\" scoped>\r\n.forum-management-container {\r\n  padding: 16px;\r\n  background-color: #f8f7fc;\r\n  height: 100%;\r\n  box-sizing: border-box;\r\n  display: flex;\r\n  flex-direction: column;\r\n  \r\n  .search-panel {\r\n    background: #ffffff;\r\n    border-radius: 8px;\r\n    padding: 16px 20px;\r\n    margin-bottom: 16px;\r\n    box-shadow: 0 1px 4px rgba(123, 104, 238, 0.08);\r\n    \r\n    .search-item {\r\n      display: flex;\r\n      flex-direction: column;\r\n      \r\n      .search-label {\r\n        font-size: 13px;\r\n        color: #666;\r\n        margin-bottom: 8px;\r\n        display: flex;\r\n        align-items: center;\r\n        \r\n        i {\r\n          margin-right: 6px;\r\n          color: #7B68EE;\r\n          font-size: 16px;\r\n        }\r\n      }\r\n    }\r\n    \r\n    .action-buttons {\r\n      display: flex;\r\n      align-items: flex-end;\r\n      height: 100%;\r\n      padding-top: 22px;\r\n      \r\n      .search-action-btn {\r\n        margin-right: 10px;\r\n      }\r\n    }\r\n  }\r\n  \r\n  .data-panel {\r\n    flex: 1;\r\n    background: #ffffff;\r\n    border-radius: 8px;\r\n    padding: 16px;\r\n    box-shadow: 0 1px 4px rgba(123, 104, 238, 0.08);\r\n    display: flex;\r\n    flex-direction: column;\r\n    \r\n    .tool-bar {\r\n      padding: 0 8px 16px;\r\n      border-bottom: 1px solid #f0f0f0;\r\n      margin-bottom: 16px;\r\n    }\r\n    \r\n    .discussion-name {\r\n      display: flex;\r\n      align-items: center;\r\n      \r\n      .name-text {\r\n        margin-left: 8px;\r\n        overflow: hidden;\r\n        text-overflow: ellipsis;\r\n        white-space: nowrap;\r\n      }\r\n    }\r\n    \r\n    .discussion-remark {\r\n      .no-remark {\r\n        color: #999;\r\n        font-size: 13px;\r\n      }\r\n    }\r\n    \r\n    .creator-info {\r\n      display: flex;\r\n      align-items: center;\r\n      \r\n      .creator-name {\r\n        margin-left: 8px;\r\n      }\r\n    }\r\n    \r\n    .create-time {\r\n      display: flex;\r\n      align-items: center;\r\n      \r\n      i {\r\n        margin-right: 6px;\r\n        color: #999;\r\n      }\r\n    }\r\n    \r\n    .row-actions {\r\n      display: flex;\r\n      \r\n      .view-btn {\r\n        color: #7B68EE;\r\n        background: rgba(123, 104, 238, 0.1);\r\n        border: none;\r\n        \r\n        &:hover {\r\n          background: rgba(123, 104, 238, 0.2);\r\n        }\r\n      }\r\n      \r\n      .delete-btn {\r\n        color: #ff4d4f;\r\n        background: rgba(255, 77, 79, 0.1);\r\n        border: none;\r\n        margin-left: 8px;\r\n        \r\n        &:hover {\r\n          background: rgba(255, 77, 79, 0.2);\r\n        }\r\n      }\r\n    }\r\n    \r\n    .pagination-wrapper {\r\n      margin-top: 20px;\r\n      display: flex;\r\n      justify-content: flex-end;\r\n    }\r\n  }\r\n}\r\n\r\n// 响应式调整\r\n@media (max-width: 768px) {\r\n  .forum-management-container {\r\n    padding: 12px;\r\n    \r\n    .search-panel {\r\n      padding: 12px;\r\n      \r\n      .action-buttons {\r\n        padding-top: 0;\r\n        justify-content: flex-start;\r\n        margin-top: 12px;\r\n      }\r\n    }\r\n    \r\n    .data-panel {\r\n      padding: 12px;\r\n    }\r\n  }\r\n}\r\n</style>\r\n\r\n<style lang=\"scss\">\r\n// 全局样式\r\n.forum-input {\r\n  .el-input__inner {\r\n    border-radius: 16px;\r\n    border: 1px solid #e0d6ff;\r\n    color: #5F4B8B;\r\n    background-color: #faf9ff;\r\n    padding-left: 32px;\r\n    transition: all 0.3s;\r\n    \r\n    &:hover {\r\n      border-color: #b8a9ff;\r\n    }\r\n    \r\n    &:focus {\r\n      border-color: #7B68EE;\r\n      box-shadow: 0 0 0 2px rgba(123, 104, 238, 0.2);\r\n    }\r\n  }\r\n  \r\n  .el-input__prefix {\r\n    left: 8px;\r\n    color: #7B68EE;\r\n  }\r\n}\r\n\r\n.search-action-btn {\r\n  background: linear-gradient(135deg, #7B68EE, #9370DB);\r\n  border: none;\r\n  border-radius: 16px;\r\n  color: white;\r\n  padding: 8px 16px;\r\n  \r\n  &:hover {\r\n    background: linear-gradient(135deg, #6A5ACD, #7B68EE);\r\n    box-shadow: 0 2px 8px rgba(123, 104, 238, 0.3);\r\n  }\r\n}\r\n\r\n.reset-action-btn {\r\n  border-radius: 16px;\r\n  color: #7B68EE;\r\n  border: 1px solid #e0d6ff;\r\n  background: #faf9ff;\r\n  padding: 8px 16px;\r\n  \r\n  &:hover {\r\n    color: #6A5ACD;\r\n    border-color: #b8a9ff;\r\n    background: #f5f2ff;\r\n  }\r\n}\r\n\r\n.batch-delete-btn {\r\n  background: linear-gradient(135deg, #ff4d4f, #ff7875);\r\n  border: none;\r\n  border-radius: 16px;\r\n  color: white;\r\n  padding: 8px 16px;\r\n  \r\n  &:hover {\r\n    background: linear-gradient(135deg, #f5222d, #ff4d4f);\r\n    box-shadow: 0 2px 8px rgba(255, 77, 79, 0.3);\r\n  }\r\n  \r\n  &:disabled {\r\n    background: #d9d9d9;\r\n    color: #bfbfbf;\r\n    cursor: not-allowed;\r\n  }\r\n}\r\n\r\n.forum-table {\r\n  border-radius: 8px;\r\n  border: 1px solid #f0ecff;\r\n  \r\