          ],\r\n          birth: [\r\n            { required: true, message: '请选择生日', trigger: 'blur' },\r\n          ],\r\n          address: [\r\n            { required: true, message: '请输入地址', trigger: 'blur' },\r\n          ],\r\n        }\r\n      }\r\n    },\r\n    props: ['updateUserVisible','updateId'],\r\n    methods: {\r\n      getApeSchoolList() {\r\n        getApeSchoolList().then(res=> {\r\n          if(res.code == 1000) {\r\n            this.school = res.data\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            });\r\n          }\r\n        })\r\n      },\r\n      getApeMajorList() {\r\n        getApeMajorList().then(res => {\r\n          if(res.code == 1000) {\r\n            this.major = res.data\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            });\r\n          }\r\n        })\r\n      },\r\n      submit() {\r\n        editUser(this.form).then(res => {\r\n          if(res.code == 1000) {\r\n            this.$notify.success({\r\n                  title: '成功',\r\n                  message: \"保存成功\"\r\n                });\r\n            this.handleClose()\r\n          } else {\r\n            this.$notify.error({\r\n              title: '错误',\r\n              message: res.message\r\n            });\r\n          }\r\n        })\r\n      },\r\n      handleClose() {\r\n        this.fileList = []\r\n        this.$emit(\"updateUserFalse\")\r\n      }\r\n    },\r\n    mounted() {\r\n      this.getApeMajorList()\r\n      this.getApeSchoolList()\r\n    },\r\n    watch: {\r\n      updateId(newVal) {\r\n        if(newVal) {\r\n          getUserById({id:newVal}).then(res => {\r\n            if(res.code == 1000) {\r\n              this.form = res.data\r\n              this.form.sex = res.data.sex + \"\"\r\n              this.form.status = res.data.status + \"\"\r\n            } else {\r\n  